var Duik_ko_KR = new DuBinary( "{\"\": {\"language\": \"ko_KR\", \"plural-forms\": \"nplurals=1; plural=0;\"}, \"Adjustment layer\": \"\uc870\uc815 \ub808\uc774\uc5b4\", \"Null\": \"\ub110\", \"Solid\": \"\uc194\ub9ac\ub4dc\", \"Animation library\": \"\uc560\ub2c8\uba54\uc774\uc158 \ub77c\uc774\ube0c\ub7ec\ub9ac\", \"Most used\": \"\uc790\uc8fc \uc0ac\uc6a9\", \"Recent\": \"\ucd5c\uadfc\", \"Favorites\": \"\uc990\uaca8\ucc3e\uae30\", \"All properties\": \"\ubaa8\ub4e0 \uc18d\uc131\", \"Keyframes only\": \"\ud0a4\ud504\ub808\uc784\ub9cc\", \"Position\": \"\uc704\uce58\", \"Rotation\": \"\ud68c\uc804\", \"Scale\": \"\ud06c\uae30\", \"Opacity\": \"\ubd88\ud22c\uba85\ub3c4\", \"Masks\": \"\ub9c8\uc2a4\ud06c\", \"Effects\": \"\ud6a8\uacfc\", \"Offset values\": \"\uc624\ud504\uc14b \uac12\", \"Offset current values.\": \"\ud604\uc7ac \uac12\uc744 \uc624\ud504\uc14b\ud569\ub2c8\ub2e4.\", \"Absolute\": \"\uc808\ub300\uac12\", \"Absolute values (replaces current values).\": \"\uc808\ub300\uac12 (\ud604\uc7ac \uac12\uc744 \ub300\uccb4\ud569\ub2c8\ub2e4).\", \"Reverse keyframes\": \"\ud0a4\ud504\ub808\uc784 \ubc18\uc804\", \"Reverses the animation in time.\": \"\uc2dc\uac04\uc0c1 \uc560\ub2c8\uba54\uc774\uc158\uc744 \ubc18\uc804\ud569\ub2c8\ub2e4.\", \"Apply\": \"\uc801\uc6a9\", \"Edit category name\": \"\uce74\ud14c\uace0\ub9ac \uc774\ub984 \ud3b8\uc9d1\", \"New Category\": \"\uc0c8 \uce74\ud14c\uace0\ub9ac\", \"Create animation\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc0dd\uc131\", \"Bake Expressions\": \"\ud45c\ud604\uc2dd \ubca0\uc774\ud06c\", \"Animation name\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc774\ub984\", \"OK\": \"\ud655\uc778\", \"Save animation.\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc800\uc7a5.\", \"This animation already exists.\nDo you want to overwrite it?\": \"\uc774 \uc560\ub2c8\uba54\uc774\uc158\uc774 \uc774\ubbf8 \uc874\uc7ac\ud569\ub2c8\ub2e4.\n\ub36e\uc5b4\uc4f0\uc2dc\uaca0\uc2b5\ub2c8\uae4c?\", \"Animation settings.\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc124\uc815.\", \"Update thumbnail\": \"\uc378\ub124\uc77c \uc5c5\ub370\uc774\ud2b8\", \"Updates the thumbnail for the selected item.\": \"\uc120\ud0dd\ud55c \ud56d\ubaa9\uc758 \uc378\ub124\uc77c\uc744 \uc5c5\ub370\uc774\ud2b8\ud569\ub2c8\ub2e4.\", \"Update animation\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc5c5\ub370\uc774\ud2b8\", \"Updates the current animation.\": \"\ud604\uc7ac \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc5c5\ub370\uc774\ud2b8\ud569\ub2c8\ub2e4.\", \"Favorite\": \"\uc990\uaca8\ucc3e\uae30\", \"Animation\": \"\uc560\ub2c8\uba54\uc774\uc158\", \"Apply selected animation.\": \"\uc120\ud0dd\ud55c \uc560\ub2c8\uba54\uc774\uc158 \uc801\uc6a9.\", \"[Shift]: More options...\": \"[Shift]: \ucd94\uac00 \uc635\uc158...\", \"Open the folder in the file explorer/finder.\": \"\ud30c\uc77c \ud0d0\uc0c9\uae30\uc5d0\uc11c \ud3f4\ub354\ub97c \uc5fd\ub2c8\ub2e4.\", \"[Alt]: Select the library location.\": \"[Alt]: \ub77c\uc774\ube0c\ub7ec\ub9ac \uc704\uce58 \uc120\ud0dd.\", \"Add selected animation to library or create new category.\": \"\uc120\ud0dd\ud55c \uc560\ub2c8\uba54\uc774\uc158\uc744 \ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0 \ucd94\uac00\ud558\uac70\ub098 \uc0c8 \uce74\ud14c\uace0\ub9ac\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Edit the selected animation or category.\": \"\uc120\ud0dd\ud55c \uc560\ub2c8\uba54\uc774\uc158 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\", \"Remove the selected animation or category from library.\": \"\ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0\uc11c \uc120\ud0dd\ud55c \uc560\ub2c8\uba54\uc774\uc158 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \uc81c\uac70\ud569\ub2c8\ub2e4.\", \"Sorry, the animation library folder can't be found.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \uc560\ub2c8\uba54\uc774\uc158 \ub77c\uc774\ube0c\ub7ec\ub9ac \ud3f4\ub354\ub97c \ucc3e\uc744 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Select the folder containing the animation library.\": \"\uc560\ub2c8\uba54\uc774\uc158 \ub77c\uc774\ube0c\ub7ec\ub9ac\uac00 \ud3ec\ud568\ub41c \ud3f4\ub354\ub97c \uc120\ud0dd\ud558\uc138\uc694.\", \"Sorry, we can't save an animation directly in the current category.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \ud604\uc7ac \uce74\ud14c\uace0\ub9ac\uc5d0 \uc9c1\uc811 \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc800\uc7a5\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Sorry, we can't create a sub-category in the current category.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \ud604\uc7ac \uce74\ud14c\uace0\ub9ac\uc5d0 \ud558\uc704 \uce74\ud14c\uace0\ub9ac\ub97c \ub9cc\ub4e4 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Uncategorized\": \"\ubbf8\ubd84\ub958\", \"Select Keyframes\": \"\ud0a4\ud504\ub808\uc784 \uc120\ud0dd\", \"Select keyframes.\": \"\ud0a4\ud504\ub808\uc784\uc744 \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Time\": \"\uc2dc\uac04\", \"Select at a precise time.\": \"\uc815\ud655\ud55c \uc2dc\uac04\uc5d0 \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Range\": \"\ubc94\uc704\", \"Select from a given range.\": \"\uc8fc\uc5b4\uc9c4 \ubc94\uc704\uc5d0\uc11c \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Current time\": \"\ud604\uc7ac \uc2dc\uac04\", \"time\": \"\uc2dc\uac04\", \"Selected layers\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\", \"All layers\": \"\ubaa8\ub4e0 \ub808\uc774\uc5b4\", \"Controllers\": \"\ucee8\ud2b8\ub864\ub7ec\", \"Copy animation\": \"\uc560\ub2c8\uba54\uc774\uc158 \ubcf5\uc0ac\", \"Copies selected keyframes.\n\n[Alt]: Cuts the selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc744 \ubcf5\uc0ac\ud569\ub2c8\ub2e4.\n\n[Alt]: \uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc744 \uc798\ub77c\ub0c5\ub2c8\ub2e4.\", \"Paste animation\": \"\uc560\ub2c8\uba54\uc774\uc158 \ubd99\uc5ec\ub123\uae30\", \"Paste keyframes.\n\n[Ctrl]: Offset from current values.\n[Alt]: Reverses the keyframes in time.\": \"\ud0a4\ud504\ub808\uc784\uc744 \ubd99\uc5ec\ub123\uc2b5\ub2c8\ub2e4.\n\n[Ctrl]: \ud604\uc7ac \uac12\uc5d0\uc11c \uc624\ud504\uc14b.\n[Alt]: \uc2dc\uac04\uc0c1 \ud0a4\ud504\ub808\uc784 \ubc18\uc804.\", \"Replace existing keyframes\": \"\uae30\uc874 \ud0a4\ud504\ub808\uc784 \uad50\uccb4\", \"Interpolator\": \"\uc778\ud130\ud3f4\ub808\uc774\ud130\", \"Control the selected keyframes with advanced but easy-to-use keyframe interpolation driven by an effect.\": \"\uc774\ud399\ud2b8\ub85c \uad6c\ub3d9\ub418\ub294 \uace0\uae09\uc774\uc9c0\ub9cc \uc0ac\uc6a9\ud558\uae30 \uc26c\uc6b4 \ud0a4\ud504\ub808\uc784 \ubcf4\uac04\uc73c\ub85c \uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc744 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Snap keys\": \"\ud0a4 \uc2a4\ub0c5\", \"Snaps selected (or all) keyframes to the closest frames if they're in between.\": \"\uc120\ud0dd\ud55c(\ub610\ub294 \ubaa8\ub4e0) \ud0a4\ud504\ub808\uc784\uc774 \ud504\ub808\uc784 \uc0ac\uc774\uc5d0 \uc788\ub294 \uacbd\uc6b0 \uac00\uc7a5 \uac00\uae4c\uc6b4 \ud504\ub808\uc784\uc73c\ub85c \uc2a4\ub0c5\ud569\ub2c8\ub2e4.\", \"Motion trail\": \"\ubaa8\uc158 \ud2b8\ub808\uc77c\", \"Draws a trail following the selected layers.\n\n[Alt]: Creates a new shape layer for each trail.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\ub97c \ub530\ub77c \uada4\uc801\uc744 \uadf8\ub9bd\ub2c8\ub2e4.\n\n[Alt]: \uac01 \uada4\uc801\uc5d0 \ub300\ud574 \uc0c8 \uc170\uc774\ud504 \ub808\uc774\uc5b4\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\", \"IK/FK Switch\": \"IK/FK \uc804\ud658\", \"Switches the selected controller between IK and FK.\nAutomatically adds the needed keyframes at current time.\": \"\uc120\ud0dd\ud55c \ucee8\ud2b8\ub864\ub7ec\ub97c IK\uc640 FK \uc0ac\uc774\uc5d0\uc11c \uc804\ud658\ud569\ub2c8\ub2e4.\n\ud604\uc7ac \uc2dc\uac04\uc5d0 \ud544\uc694\ud55c \ud0a4\ud504\ub808\uc784\uc744 \uc790\ub3d9\uc73c\ub85c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Snap IK\": \"IK \uc2a4\ub0c5\", \"Snaps the IK to the FK values\": \"IK\ub97c FK \uac12\uc73c\ub85c \uc2a4\ub0c5\ud569\ub2c8\ub2e4\", \"Snap FK\": \"FK \uc2a4\ub0c5\", \"Snaps the FK to the IK values\": \"FK\ub97c IK \uac12\uc73c\ub85c \uc2a4\ub0c5\ud569\ub2c8\ub2e4\", \"Tweening\": \"\ud2b8\uc704\ub2dd\", \"Split\": \"\ubd84\ud560\", \"Split the selected keyframes into couples of keyframes with the same value.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc744 \uac19\uc740 \uac12\uc758 \ud0a4\ud504\ub808\uc784 \uc30d\uc73c\ub85c \ubd84\ud560\ud569\ub2c8\ub2e4.\", \"Duration\": \"\uc9c0\uc18d \uc2dc\uac04\", \"Set the duration between two split keys.\": \"\ubd84\ud560 \ud0a4 \uc0ac\uc774\uc758 \uc9c0\uc18d \uc2dc\uac04\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Center\": \"\uc911\uc559\", \"Align around the current time.\": \"\ud604\uc7ac \uc2dc\uac04 \uc8fc\uc704\ub85c \uc815\ub82c\ud569\ub2c8\ub2e4.\", \"After\": \"\uc774\ud6c4\", \"Add the new key after the current one.\": \"\ud604\uc7ac \ud0a4 \ub4a4\uc5d0 \uc0c8 \ud0a4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Before\": \"\uc774\uc804\", \"Add the new key before the current one.\": \"\ud604\uc7ac \ud0a4 \uc55e\uc5d0 \uc0c8 \ud0a4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Freeze\": \"\ud504\ub9ac\uc988\", \"Freezes the pose; copies the previous keyframe to the current time.\n\n[Alt]: Freezes the next pose (copies the next keyframe to the current time).\": \"\ud3ec\uc988\ub97c \ud504\ub9ac\uc988\ud569\ub2c8\ub2e4; \uc774\uc804 \ud0a4\ud504\ub808\uc784\uc744 \ud604\uc7ac \uc2dc\uac04\uc73c\ub85c \ubcf5\uc0ac\ud569\ub2c8\ub2e4.\n\n[Alt]: \ub2e4\uc74c \ud3ec\uc988\ub97c \ud504\ub9ac\uc988\ud569\ub2c8\ub2e4 (\ub2e4\uc74c \ud0a4\ud504\ub808\uc784\uc744 \ud604\uc7ac \uc2dc\uac04\uc73c\ub85c \ubcf5\uc0ac).\", \"Animated properties\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc18d\uc131\", \"Selected properties\": \"\uc120\ud0dd\ud55c \uc18d\uc131\", \"Sync\": \"\ub3d9\uae30\ud654\", \"Synchronize the selected keyframes; moves them to the current time.\nIf multiple keyframes are selected for the same property, they're offset to the current time, keeping the animation.\n\n[Alt]: Syncs using the last keyframe instead of the first.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc744 \ub3d9\uae30\ud654\ud569\ub2c8\ub2e4; \ud604\uc7ac \uc2dc\uac04\uc73c\ub85c \uc774\ub3d9\ud569\ub2c8\ub2e4.\n\uac19\uc740 \uc18d\uc131\uc5d0 \uc5ec\ub7ec \ud0a4\ud504\ub808\uc784\uc774 \uc120\ud0dd\ub41c \uacbd\uc6b0, \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc720\uc9c0\ud558\uba70 \ud604\uc7ac \uc2dc\uac04\uc73c\ub85c \uc624\ud504\uc14b\ud569\ub2c8\ub2e4.\n\n[Alt]: \uccab \ubc88\uc9f8 \ub300\uc2e0 \ub9c8\uc9c0\ub9c9 \ud0a4\ud504\ub808\uc784\uc73c\ub85c \ub3d9\uae30\ud654\ud569\ub2c8\ub2e4.\", \"Clean\": \"\uc815\ub9ac\", \"Remove unneeded keyframes.\": \"\ubd88\ud544\uc694\ud55c \ud0a4\ud504\ub808\uc784\uc744 \uc81c\uac70\ud569\ub2c8\ub2e4.\", \"Tweening options\": \"\ud2b8\uc704\ub2dd \uc635\uc158\", \"Temporal interpolation\": \"\uc2dc\uac04 \ubcf4\uac04\", \"Set key options\": \"\ud0a4 \uc635\uc158 \uc124\uc815\", \"Roving\": \"\ub85c\ube59\", \"Linear\": \"\uc120\ud615\", \"Ease In\": \"\uc774\uc988 \uc778\", \"Ease Out\": \"\uc774\uc988 \uc544\uc6c3\", \"Easy Ease\": \"\uc774\uc9c0 \uc774\uc988\", \"Continuous\": \"\uc5f0\uc18d\", \"Hold\": \"\ud640\ub4dc\", \"Keyframe options\": \"\ud0a4\ud504\ub808\uc784 \uc635\uc158\", \"Add keyframes\": \"\ud0a4\ud504\ub808\uc784 \ucd94\uac00\", \"Edit selected keyframes\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784 \ud3b8\uc9d1\", \"Ease options\": \"\uc774\uc988 \uc635\uc158\", \"Reset preset list\": \"\ud504\ub9ac\uc14b \ubaa9\ub85d \ucd08\uae30\ud654\", \"Resets the preset list to the default values.\": \"\ud504\ub9ac\uc14b \ubaa9\ub85d\uc744 \uae30\ubcf8\uac12\uc73c\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\", \"Ease presets\": \"\uc774\uc988 \ud504\ub9ac\uc14b\", \"Add new ease preset\": \"\uc0c8 \uc774\uc988 \ud504\ub9ac\uc14b \ucd94\uac00\", \"Remove selected ease preset\": \"\uc120\ud0dd\ud55c \uc774\uc988 \ud504\ub9ac\uc14b \uc81c\uac70\", \"Pick ease and velocity from selected key.\": \"\uc120\ud0dd\ud55c \ud0a4\uc5d0\uc11c \uc774\uc988\uc640 \uc18d\ub3c4\ub97c \uac00\uc838\uc635\ub2c8\ub2e4.\", \"Apply ease and velocity to selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc5d0 \uc774\uc988\uc640 \uc18d\ub3c4\ub97c \uc801\uc6a9\ud569\ub2c8\ub2e4.\", \"Set ease\": \"\uc774\uc988 \uc124\uc815\", \"Switches in and out eases.\": \"\uc778/\uc544\uc6c3 \uc774\uc988\ub97c \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Apply ease to selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc5d0 \uc774\uc988\ub97c \uc801\uc6a9\ud569\ub2c8\ub2e4.\", \"Switches in and out velocities.\": \"\uc778/\uc544\uc6c3 \uc18d\ub3c4\ub97c \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Apply velocity to selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc5d0 \uc18d\ub3c4\ub97c \uc801\uc6a9\ud569\ub2c8\ub2e4.\", \"Spatial interpolation\": \"\uacf5\uac04 \ubcf4\uac04\", \"Set the spatial interpolation to linear for selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc758 \uacf5\uac04 \ubcf4\uac04\uc744 \uc120\ud615\uc73c\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Set the spatial interpolation to B\u00e9zier for selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc758 \uacf5\uac04 \ubcf4\uac04\uc744 \ubca0\uc9c0\uc5b4\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Set the spatial interpolation to B\u00e9zier Out for selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc758 \uacf5\uac04 \ubcf4\uac04\uc744 \ubca0\uc9c0\uc5b4 \uc544\uc6c3\uc73c\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Set the spatial interpolation to B\u00e9zier In for selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc758 \uacf5\uac04 \ubcf4\uac04\uc744 \ubca0\uc9c0\uc5b4 \uc778\uc73c\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Fix\": \"\uc218\uc815\", \"Automatically fix spatial interpolation for selected keyframes.\": \"\uc120\ud0dd\ud55c \ud0a4\ud504\ub808\uc784\uc758 \uacf5\uac04 \ubcf4\uac04\uc744 \uc790\ub3d9\uc73c\ub85c \uc218\uc815\ud569\ub2c8\ub2e4.\", \"Quickly save, export, import and apply animations from predefined folders.\": \"\ubbf8\ub9ac \uc815\uc758\ub41c \ud3f4\ub354\uc5d0\uc11c \uc560\ub2c8\uba54\uc774\uc158\uc744 \ube60\ub974\uac8c \uc800\uc7a5, \ub0b4\ubcf4\ub0b4\uae30, \uac00\uc838\uc624\uae30 \ubc0f \uc801\uc6a9\ud569\ub2c8\ub2e4.\", \"Sequence\": \"\uc2dc\ud000\uc2a4\", \"Sequence layers or keyframes.\n\n[Ctrl]: Sequence keyframes instead of layers\n[Alt]: Reverse\": \"\ub808\uc774\uc5b4 \ub610\ub294 \ud0a4\ud504\ub808\uc784\uc744 \uc2dc\ud000\uc2a4\ud569\ub2c8\ub2e4.\n\n[Ctrl]: \ub808\uc774\uc5b4 \ub300\uc2e0 \ud0a4\ud504\ub808\uc784\uc744 \uc2dc\ud000\uc2a4\n[Alt]: \ubc18\uc804\", \"Layers\": \"\ub808\uc774\uc5b4\", \"Keyframes\": \"\ud0a4\ud504\ub808\uc784\", \"Times\": \"\uc2dc\uac04\", \"In points\": \"\uc778 \ud3ec\uc778\ud2b8\", \"Out points\": \"\uc544\uc6c3 \ud3ec\uc778\ud2b8\", \"Ease - Sigmoid (logistic)\": \"\uc774\uc988 - \uc2dc\uadf8\ubaa8\uc774\ub4dc (\ub85c\uc9c0\uc2a4\ud2f1)\", \"Natural - Bell (gaussian)\": \"\uc790\uc5f0 - \ubca8 (\uac00\uc6b0\uc2dc\uc548)\", \"Ease In (logarithmic)\": \"\uc774\uc988 \uc778 (\ub85c\uadf8)\", \"Ease Out (exponential)\": \"\uc774\uc988 \uc544\uc6c3 (\uc9c0\uc218)\", \"Non-linear animation\": \"\ube44\uc120\ud615 \uc560\ub2c8\uba54\uc774\uc158\", \"Edit animations together.\": \"\uc560\ub2c8\uba54\uc774\uc158\uc744 \ud568\uaed8 \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\", \"Add new clip\": \"\uc0c8 \ud074\ub9bd \ucd94\uac00\", \"Create a new clip from the original comp and adds it to the 'NLA.Edit' comp.\": \"\uc6d0\ubcf8 \ucef4\ud504\uc5d0\uc11c \uc0c8 \ud074\ub9bd\uc744 \ub9cc\ub4e4\uc5b4 'NLA.Edit' \ucef4\ud504\uc5d0 \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Cel animation...\": \"\uc140 \uc560\ub2c8\uba54\uc774\uc158...\", \"Tools to help traditionnal animation using After Effects' paint effect with the brush tool.\": \"After Effects\uc758 \ud398\uc778\ud2b8 \ud6a8\uacfc\uc640 \ube0c\ub7ec\uc2dc \ub3c4\uad6c\ub97c \uc0ac\uc6a9\ud55c \uc804\ud1b5 \uc560\ub2c8\uba54\uc774\uc158 \ubcf4\uc870 \ub3c4\uad6c.\", \"Cel animation\": \"\uc140 \uc560\ub2c8\uba54\uc774\uc158\", \"New Cel.\": \"\uc0c8 \uc140.\", \"Create a new animation cel.\n\n[Alt]: Creates on the selected layer instead of adding a new layer.\": \"\uc0c8 \uc560\ub2c8\uba54\uc774\uc158 \uc140\uc744 \ub9cc\ub4ed\ub2c8\ub2e4.\n\n[Alt]: \uc0c8 \ub808\uc774\uc5b4\ub97c \ucd94\uac00\ud558\ub294 \ub300\uc2e0 \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc5d0 \ub9cc\ub4ed\ub2c8\ub2e4.\", \"Onion skin\": \"\uc5b4\ub2c8\uc5b8 \uc2a4\ud0a8\", \"Shows the previous and next frames with a reduced opacity.\": \"\uc774\uc804 \ubc0f \ub2e4\uc74c \ud504\ub808\uc784\uc744 \ub0ae\uc740 \ubd88\ud22c\uba85\ub3c4\ub85c \ud45c\uc2dc\ud569\ub2c8\ub2e4.\", \"Go to the previous frame\": \"\uc774\uc804 \ud504\ub808\uc784\uc73c\ub85c \uc774\ub3d9\", \"Changes the exposure of the animation (the frames per second).\": \"\uc560\ub2c8\uba54\uc774\uc158\uc758 \ub178\ucd9c(\ucd08\ub2f9 \ud504\ub808\uc784)\uc744 \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Go to the next frame.\": \"\ub2e4\uc74c \ud504\ub808\uc784\uc73c\ub85c \uc774\ub3d9.\", \"Cut animation\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc798\ub77c\ub0b4\uae30\", \"Set velocity\": \"\uc18d\ub3c4 \uc124\uc815\", \"Tween\": \"\ud2b8\uc708\", \"Celluloid\": \"\uc140\ub8f0\ub85c\uc774\ub4dc\", \"X-Sheet\": \"X-\uc2dc\ud2b8\", \"Clean keyframes\": \"\ud0a4\ud504\ub808\uc784 \uc815\ub9ac\", \"Weight\": \"\uac00\uc911\uce58\", \"Looper\": \"\ub8e8\ud37c\", \"Paste expression\": \"\ud45c\ud604\uc2dd \ubd99\uc5ec\ub123\uae30\", \"Remove expressions\": \"\ud45c\ud604\uc2dd \uc81c\uac70\", \"Toggle expressions\": \"\ud45c\ud604\uc2dd \uc804\ud658\", \"Bake expressions\": \"\ud45c\ud604\uc2dd \ubca0\uc774\ud06c\", \"Bake composition\": \"\ucef4\ud3ec\uc9c0\uc158 \ubca0\uc774\ud06c\", \"Effector\": \"\uc774\ud399\ud130\", \"Effector map\": \"\uc774\ud399\ud130 \ub9f5\", \"Kleaner\": \"\ud074\ub9ac\ub108\", \"Swink\": \"\uc2a4\uc719\ud06c\", \"Wiggle\": \"\uc704\uae00\", \"Random motion\": \"\ub79c\ub364 \ubaa8\uc158\", \"Random\": \"\ub79c\ub364\", \"Wheel\": \"\ud720\", \"Move away\": \"\uba40\ub9ac \uc774\ub3d9\", \"Adds a control effect to move the selected layers away from their parents.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\ub97c \ubd80\ubaa8\uc5d0\uc11c \uba40\ub9ac \uc774\ub3d9\uc2dc\ud0a4\ub294 \uc81c\uc5b4 \ud6a8\uacfc\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Randomize\": \"\ub79c\ub364\ud654\", \"Motion trails\": \"\ubaa8\uc158 \ud2b8\ub808\uc77c\", \"Time remap\": \"\uc2dc\uac04 \ub9ac\ub9f5\", \"Paint Rig\": \"\ud398\uc778\ud2b8 \ub9ac\uadf8\", \"Walk/Run cycle\": \"\uac77\uae30/\ub2ec\ub9ac\uae30 \uc0ac\uc774\ud074\", \"Arm\": \"\ud314\", \"Limb\": \"\uc0ac\uc9c0\", \"Leg\": \"\ub2e4\ub9ac\", \"Spine\": \"\ucc99\ucd94\", \"Tail\": \"\uaf2c\ub9ac\", \"Hair\": \"\uba38\ub9ac\uce74\ub77d\", \"Wing\": \"\ub0a0\uac1c\", \"Fin\": \"\uc9c0\ub290\ub7ec\ubbf8\", \"Bake armature data\": \"\uc544\ub9c8\ucd94\uc5b4 \ub370\uc774\ud130 \ubca0\uc774\ud06c\", \"Baking armature data...\": \"\uc544\ub9c8\ucd94\uc5b4 \ub370\uc774\ud130 \ubca0\uc774\ud06c \uc911...\", \"Baking armature data: %1\": \"\uc544\ub9c8\ucd94\uc5b4 \ub370\uc774\ud130 \ubca0\uc774\ud06c \uc911: %1\", \"Bake bones\": \"\ubf08\ub300 \ubca0\uc774\ud06c\", \"Resetting bone transform...\": \"\ubf08\ub300 \ubcc0\ud658 \ucd08\uae30\ud654 \uc911...\", \"Baking bone: %1\": \"\ubf08\ub300 \ubca0\uc774\ud06c \uc911: %1\", \"Link art\": \"\uc544\ud2b8 \uc5f0\uacb0\", \"Trying to parent layer '%1'\": \"\ub808\uc774\uc5b4 '%1' \ubd80\ubaa8 \uc5f0\uacb0 \uc2dc\ub3c4 \uc911\", \"Framing guides\": \"\ud504\ub808\uc774\ubc0d \uac00\uc774\ub4dc\", \"Scale Z-Link\": \"\uc2a4\ucf00\uc77c Z-\ub9c1\ud06c\", \"Camera Rig\": \"\uce74\uba54\ub77c \ub9ac\uadf8\", \"Camera\": \"\uce74\uba54\ub77c\", \"Target\": \"\ud0c0\uac9f\", \"Cam\": \"\ucea0\", \"2D Camera\": \"2D \uce74\uba54\ub77c\", \"Camera influence\": \"\uce74\uba54\ub77c \uc601\ud5a5\", \"List\": \"\ubaa9\ub85d\", \"Add list\": \"\ubaa9\ub85d \ucd94\uac00\", \"Split values\": \"\uac12 \ubd84\ub9ac\", \"Lock properties\": \"\uc18d\uc131 \uc7a0\uae08\", \"Add zero\": \"\uc81c\ub85c \ucd94\uac00\", \"Move anchor points\": \"\uc575\ucee4 \ud3ec\uc778\ud2b8 \uc774\ub3d9\", \"Reset transformation\": \"\ubcc0\ud658 \ucd08\uae30\ud654\", \"Align layers\": \"\ub808\uc774\uc5b4 \uc815\ub82c\", \"Expose transform\": \"\ubcc0\ud658 \ub178\ucd9c\", \"Key Morph\": \"\ud0a4 \ubaa8\ud504\", \"Tip angle\": \"\ud301 \uac01\ub3c4\", \"B\u00e9zier IK\": \"\ubca0\uc9c0\uc5b4 IK\", \"B\u00e9zier FK\": \"\ubca0\uc9c0\uc5b4 FK\", \"Auto-curve\": \"\uc790\ub3d9 \ucee4\ube0c\", \"Auto-parent\": \"\uc790\ub3d9 \ubd80\ubaa8 \uc5f0\uacb0\", \"Parent constraint\": \"\ubd80\ubaa8 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Locator\": \"\ub85c\ucf00\uc774\ud130\", \"Extract locators\": \"\ub85c\ucf00\uc774\ud130 \ucd94\ucd9c\", \"Parent across comps\": \"\ucef4\ud504 \uac04 \ubd80\ubaa8 \uc5f0\uacb0\", \"Position constraint\": \"\uc704\uce58 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Orientation constraint\": \"\ubc29\ud5a5 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Path constraint\": \"\ud328\uc2a4 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Add Pins\": \"\ud540 \ucd94\uac00\", \"Remove 'thisComp'\": \"'thisComp' \uc81c\uac70\", \"Use 'thisComp'\": \"'thisComp' \uc0ac\uc6a9\", \"Connector\": \"\ucee4\ub125\ud130\", \"Audio connector\": \"\uc624\ub514\uc624 \ucee4\ub125\ud130\", \"Settings\": \"\uc124\uc815\", \"Audio spectrum\": \"\uc624\ub514\uc624 \uc2a4\ud399\ud2b8\ub7fc\", \"Audio Effector\": \"\uc624\ub514\uc624 \uc774\ud399\ud130\", \"Create controller\": \"\ucee8\ud2b8\ub864\ub7ec \uc0dd\uc131\", \"Slider\": \"\uc2ac\ub77c\uc774\ub354\", \"Controller\": \"\ucee8\ud2b8\ub864\ub7ec\", \"Hand\": \"\uc190\", \"X Position\": \"X \uc704\uce58\", \"Y Position\": \"Y \uc704\uce58\", \"Transform\": \"\ubcc0\ud658\", \"Eye\": \"\ub208\", \"Head\": \"\uba38\ub9ac\", \"Hips\": \"\uc5c9\ub369\uc774\", \"Body\": \"\ubab8\uccb4\", \"Shoulders\": \"\uc5b4\uae68\", \"Foot\": \"\ubc1c\", \"Ear\": \"\uadc0\", \"Mouth\": \"\uc785\", \"Nose\": \"\ucf54\", \"Eyebrow\": \"\ub208\uc379\", \"Claws\": \"\ubc1c\ud1b1\", \"Hoof\": \"\ubc1c\uad7d\", \"Pincer\": \"\uc9d1\uac8c\", \"Audio\": \"\uc624\ub514\uc624\", \"Torso\": \"\ubab8\ud1b5\", \"Vertebrae\": \"\ucc99\ucd94\ubf08\", \"Animation Cycles\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc0ac\uc774\ud074\", \"Animation Blender\": \"\uc560\ub2c8\uba54\uc774\uc158 \ube14\ub80c\ub354\", \"Expose Transform\": \"\ubcc0\ud658 \ub178\ucd9c\", \"Bake controllers\": \"\ucee8\ud2b8\ub864\ub7ec \ubca0\uc774\ud06c\", \"Extract controllers\": \"\ucee8\ud2b8\ub864\ub7ec \ucd94\ucd9c\", \"No new controller was found to extract.\nDo you want to extract all controllers from this pre-composition anyway?\": \"\ucd94\ucd9c\ud560 \uc0c8 \ucee8\ud2b8\ub864\ub7ec\ub97c \ucc3e\uc744 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\n\uadf8\ub798\ub3c4 \uc774 \ud504\ub9ac\ucef4\ud504\uc5d0\uc11c \ubaa8\ub4e0 \ucee8\ud2b8\ub864\ub7ec\ub97c \ucd94\ucd9c\ud558\uc2dc\uaca0\uc2b5\ub2c8\uae4c?\", \"Nothing new!\": \"\uc0c8\ub85c\uc6b4 \uac83\uc774 \uc5c6\uc2b5\ub2c8\ub2e4!\", \"Tag as ctrls\": \"\ucee8\ud2b8\ub864\ub7ec\ub85c \ud0dc\uadf8\", \"Left\": \"\uc67c\ucabd\", \"Right\": \"\uc624\ub978\ucabd\", \"Front\": \"\uc55e\", \"Back\": \"\ub4a4\", \"Middle\": \"\uc911\uac04\", \"Under\": \"\uc544\ub798\", \"Above\": \"\uc704\", \"Toggle edit mode\": \"\ud3b8\uc9d1 \ubaa8\ub4dc \uc804\ud658\", \"Bone\": \"\ubf08\ub300\", \"Pin\": \"\ud540\", \"Zero\": \"\uc81c\ub85c\", \"Building OCO character:\": \"OCO \uce90\ub9ad\ud130 \ube4c\ub4dc \uc911:\", \"Sorting bones...\": \"\ubf08\ub300 \uc815\ub82c \uc911...\", \"Lock tangents\": \"\ud0c4\uc820\ud2b8 \uc7a0\uae08\", \"Some layers are 3D layers, that's a problem...\": \"\uc77c\ubd80 \ub808\uc774\uc5b4\uac00 3D \ub808\uc774\uc5b4\uc785\ub2c8\ub2e4. \ubb38\uc81c\uac00 \ubc1c\uc0dd\ud569\ub2c8\ub2e4...\", \"This can't be rigged, sorry.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \ub9ac\uae45\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Auto-rig\": \"\uc790\ub3d9 \ub9ac\uadf8\", \"Sorting layers...\": \"\ub808\uc774\uc5b4 \uc815\ub82c \uc911...\", \"Root\": \"\ub8e8\ud2b8\", \"Shoulders & neck\": \"\uc5b4\uae68 & \ubaa9\", \"Heel\": \"\ubc1c\ub4a4\uafc8\uce58\", \"Shoulder\": \"\uc5b4\uae68\", \"Front leg\": \"\uc55e\ub2e4\ub9ac\", \"Creating controllers\": \"\ucee8\ud2b8\ub864\ub7ec \uc0dd\uc131 \uc911\", \"Parenting...\": \"\ubd80\ubaa8 \uc5f0\uacb0 \uc911...\", \"Fish spine\": \"\ubb3c\uace0\uae30 \ucc99\ucd94\", \"Rigging...\": \"\ub9ac\uae45 \uc911...\", \"Custom bones\": \"\ucee4\uc2a4\ud140 \ubf08\ub300\", \"Crop precompositions\": \"\ud504\ub9ac\ucef4\ud504 \uc790\ub974\uae30\", \"Edit expression\": \"\ud45c\ud604\uc2dd \ud3b8\uc9d1\", \"A higher factor \u2192 a higher precision.\n\n\u2022 Smart mode: lower the factor to keep keyframes only for extreme values. Increasing the factor helps to get a more precise curve with inflexion keyframes.\n\n\u2022 Precise mode: A factor higher than 1.0 increases the precision to sub-frame sampling (2 \u2192 two samples per frame).\nA factor lower than 1.0 decreases the precision so that less frames are sampled (0.5 \u2192 half of the frames are sampled).\nDecrease the precision to make the process faster, increase it if you need a more precise motion-blur, for example.\": \"\ub192\uc740 \uacc4\uc218 \u2192 \ub192\uc740 \uc815\ubc00\ub3c4.\n\n\u2022 \uc2a4\ub9c8\ud2b8 \ubaa8\ub4dc: \uadf9\uac12\uc5d0\ub9cc \ud0a4\ud504\ub808\uc784\uc744 \uc720\uc9c0\ud558\ub824\uba74 \uacc4\uc218\ub97c \ub0ae\ucd94\uc138\uc694. \uacc4\uc218\ub97c \ub192\uc774\uba74 \ubcc0\uace1 \ud0a4\ud504\ub808\uc784\uc73c\ub85c \ub354 \uc815\ubc00\ud55c \ucee4\ube0c\ub97c \uc5bb\uc744 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\n\u2022 \uc815\ubc00 \ubaa8\ub4dc: 1.0\ubcf4\ub2e4 \ub192\uc740 \uacc4\uc218\ub294 \uc11c\ube0c\ud504\ub808\uc784 \uc0d8\ud50c\ub9c1 \uc815\ubc00\ub3c4\ub97c \ub192\uc785\ub2c8\ub2e4 (2 \u2192 \ud504\ub808\uc784\ub2f9 2\uac1c \uc0d8\ud50c).\n1.0\ubcf4\ub2e4 \ub0ae\uc740 \uacc4\uc218\ub294 \uc0d8\ud50c\ub9c1 \ud504\ub808\uc784 \uc218\ub97c \uc904\uc785\ub2c8\ub2e4 (0.5 \u2192 \ud504\ub808\uc784\uc758 \uc808\ubc18).\n\ucc98\ub9ac \uc18d\ub3c4\ub97c \ub192\uc774\ub824\uba74 \uc815\ubc00\ub3c4\ub97c \ub0ae\ucd94\uace0, \ub354 \uc815\ubc00\ud55c \ubaa8\uc158 \ube14\ub7ec \ub4f1\uc774 \ud544\uc694\ud558\uba74 \ub192\uc774\uc138\uc694.\", \"Automation and expressions\": \"\uc790\ub3d9\ud654 \ubc0f \ud45c\ud604\uc2dd\", \"Separate the dimensions of the selected properties.\nAlso works with colors, separated to RGB or HSL.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc758 \ucc28\uc6d0\uc744 \ubd84\ub9ac\ud569\ub2c8\ub2e4.\n\uc0c9\uc0c1\ub3c4 RGB \ub610\ub294 HSL\ub85c \ubd84\ub9ac \uac00\ub2a5\ud569\ub2c8\ub2e4.\", \"Toggle expressions, or remove them keeping the post-expression value on all selected properties.\n\n[Ctrl]: Remove expressions instead of just disabling.\n[Alt]: Remove expressions but keep the pre-expression value (After Effects default).\": \"\uc120\ud0dd\ud55c \ubaa8\ub4e0 \uc18d\uc131\uc758 \ud45c\ud604\uc2dd\uc744 \uc804\ud658\ud558\uac70\ub098, \ud45c\ud604\uc2dd \ud6c4 \uac12\uc744 \uc720\uc9c0\ud558\uba70 \uc81c\uac70\ud569\ub2c8\ub2e4.\n\n[Ctrl]: \ube44\ud65c\uc131\ud654 \ub300\uc2e0 \ud45c\ud604\uc2dd\uc744 \uc81c\uac70\ud569\ub2c8\ub2e4.\n[Alt]: \ud45c\ud604\uc2dd\uc744 \uc81c\uac70\ud558\ub418 \ud45c\ud604\uc2dd \uc804 \uac12\uc744 \uc720\uc9c0\ud569\ub2c8\ub2e4 (After Effects \uae30\ubcf8\uac12).\", \"Expression tools\": \"\ud45c\ud604\uc2dd \ub3c4\uad6c\", \"Various tools to fix and work with expressions\": \"\ud45c\ud604\uc2dd \uc218\uc815 \ubc0f \uc791\uc5c5\uc744 \uc704\ud55c \ub2e4\uc591\ud55c \ub3c4\uad6c\", \"Remove\": \"\uc81c\uac70\", \"Replace all occurences of %1 by %2.\": \"%1\uc758 \ubaa8\ub4e0 \ud56d\ubaa9\uc744 %2\ub85c \ub300\uccb4\ud569\ub2c8\ub2e4.\", \"Use\": \"\uc0ac\uc6a9\", \"Copy expression\": \"\ud45c\ud604\uc2dd \ubcf5\uc0ac\", \"Copy the expression from the selected property.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc5d0\uc11c \ud45c\ud604\uc2dd\uc744 \ubcf5\uc0ac\ud569\ub2c8\ub2e4.\", \"Paste the expression in all selected properties.\": \"\uc120\ud0dd\ud55c \ubaa8\ub4e0 \uc18d\uc131\uc5d0 \ud45c\ud604\uc2dd\uc744 \ubd99\uc5ec\ub123\uc2b5\ub2c8\ub2e4.\", \"Use an external editor to edit the selected expression.\n\n[Ctrl]: Reloads the expressions from the external editor.\": \"\uc678\ubd80 \ud3b8\uc9d1\uae30\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc120\ud0dd\ud55c \ud45c\ud604\uc2dd\uc744 \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\n\n[Ctrl]: \uc678\ubd80 \ud3b8\uc9d1\uae30\uc5d0\uc11c \ud45c\ud604\uc2dd\uc744 \ub2e4\uc2dc \ubd88\ub7ec\uc635\ub2c8\ub2e4.\", \"Open expressions with...\": \"\ud45c\ud604\uc2dd \uc5f4\uae30...\", \"Select an application to open the expressions.\nLeave the field empty to use the system default for '.jsxinc' files.\": \"\ud45c\ud604\uc2dd\uc744 \uc5f4 \uc560\ud50c\ub9ac\ucf00\uc774\uc158\uc744 \uc120\ud0dd\ud558\uc138\uc694.\n'.jsxinc' \ud30c\uc77c\uc758 \uc2dc\uc2a4\ud15c \uae30\ubcf8\uac12\uc744 \uc0ac\uc6a9\ud558\ub824\uba74 \ube44\uc6cc\ub450\uc138\uc694.\", \"System default\": \"\uc2dc\uc2a4\ud15c \uae30\ubcf8\uac12\", \"Set a random value to the selected properties, keyframes or layers.\": \"\uc120\ud0dd\ud55c \uc18d\uc131, \ud0a4\ud504\ub808\uc784 \ub610\ub294 \ub808\uc774\uc5b4\uc5d0 \ub79c\ub364 \uac12\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Current values\": \"\ud604\uc7ac \uac12\", \"Values of the properties at the current time\": \"\ud604\uc7ac \uc2dc\uac04\uc758 \uc18d\uc131 \uac12\", \"Layer attributes\": \"\ub808\uc774\uc5b4 \uc18d\uc131\", \"Attributes of the layers.\": \"\ub808\uc774\uc5b4\uc758 \uc18d\uc131.\", \"Keyframes (value and time).\": \"\ud0a4\ud504\ub808\uc784 (\uac12\uacfc \uc2dc\uac04).\", \"Natural (gaussian)\": \"\uc790\uc5f0 (\uac00\uc6b0\uc2dc\uc548)\", \"Uses an algorithm with a natural result (using the Gaussian bell-shaped function).\nA few values may be out of range.\": \"\uc790\uc5f0\uc2a4\ub7ec\uc6b4 \uacb0\uacfc\ub97c \uc704\ud55c \uc54c\uace0\ub9ac\uc998\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4 (\uac00\uc6b0\uc2dc\uc548 \ubca8 \ud568\uc218).\n\uc77c\ubd80 \uac12\uc774 \ubc94\uc704\ub97c \ubc97\uc5b4\ub0a0 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Strict\": \"\uc5c4\uaca9\", \"Uses strict values.\": \"\uc5c4\uaca9\ud55c \uac12\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Collapse dimensions\": \"\ucc28\uc6d0 \ubcd1\ud569\", \"Controls all dimensions or channels with a single value.\": \"\ub2e8\uc77c \uac12\uc73c\ub85c \ubaa8\ub4e0 \ucc28\uc6d0\uc774\ub098 \ucc44\ub110\uc744 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Separate all dimensions or channels to control them individually.\": \"\ubaa8\ub4e0 \ucc28\uc6d0\uc774\ub098 \ucc44\ub110\uc744 \ubd84\ub9ac\ud558\uc5ec \uac1c\ubcc4 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Indices\": \"\uc778\ub371\uc2a4\", \"Values\": \"\uac12\", \"Control all dimensions or channels with a single value.\": \"\ub2e8\uc77c \uac12\uc73c\ub85c \ubaa8\ub4e0 \ucc28\uc6d0\uc774\ub098 \ucc44\ub110\uc744 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Min\": \"\ucd5c\uc18c\", \"Max\": \"\ucd5c\ub300\", \"Replace expressions by keyframes.\nUse a smart algorithm to have as less keyframes as possible, and keep them easy to edit afterwards.\": \"\ud45c\ud604\uc2dd\uc744 \ud0a4\ud504\ub808\uc784\uc73c\ub85c \ub300\uccb4\ud569\ub2c8\ub2e4.\n\uc2a4\ub9c8\ud2b8 \uc54c\uace0\ub9ac\uc998\uc73c\ub85c \uac00\ub2a5\ud55c \uc801\uc740 \ud0a4\ud504\ub808\uc784\uc744 \uc720\uc9c0\ud558\uba70 \ub098\uc911\uc5d0 \ud3b8\uc9d1\ud558\uae30 \uc27d\uac8c \ud569\ub2c8\ub2e4.\", \"Smart mode\": \"\uc2a4\ub9c8\ud2b8 \ubaa8\ub4dc\", \"Use a smarter algorithm which produces less keyframes.\nThe result may be easier to edit afterwards but a bit less precise than other modes\": \"\ub354 \uc801\uc740 \ud0a4\ud504\ub808\uc784\uc744 \uc0dd\uc131\ud558\ub294 \uc2a4\ub9c8\ud2b8 \uc54c\uace0\ub9ac\uc998\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\n\uacb0\uacfc\uac00 \ud3b8\uc9d1\ud558\uae30 \uc27d\uc9c0\ub9cc \ub2e4\ub978 \ubaa8\ub4dc\ubcf4\ub2e4 \ub35c \uc815\ubc00\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Precise mode\": \"\uc815\ubc00 \ubaa8\ub4dc\", \"Add new keyframes for all frames.\nThis mode produces more keyframes but the result may be closer to the original animation.\": \"\ubaa8\ub4e0 \ud504\ub808\uc784\uc5d0 \uc0c8 \ud0a4\ud504\ub808\uc784\uc744 \ucd94\uac00\ud569\ub2c8\ub2e4.\n\uc774 \ubaa8\ub4dc\ub294 \ub354 \ub9ce\uc740 \ud0a4\ud504\ub808\uc784\uc744 \uc0dd\uc131\ud558\uc9c0\ub9cc \uc6d0\ubcf8 \uc560\ub2c8\uba54\uc774\uc158\uc5d0 \ub354 \uac00\uae4c\uc6b8 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Precision factor\": \"\uc815\ubc00\ub3c4 \uacc4\uc218\", \"Replaces all expressions of the composition by keyframes,\nand removes all non-renderable layers.\n\nUses a smart algorithm to have as less keyframes as possible, and keep them easy to edit afterwards.\": \"\ucef4\ud3ec\uc9c0\uc158\uc758 \ubaa8\ub4e0 \ud45c\ud604\uc2dd\uc744 \ud0a4\ud504\ub808\uc784\uc73c\ub85c \ub300\uccb4\ud558\uace0,\n\ub80c\ub354\ub9c1 \ubd88\uac00\ub2a5\ud55c \ubaa8\ub4e0 \ub808\uc774\uc5b4\ub97c \uc81c\uac70\ud569\ub2c8\ub2e4.\n\n\uc2a4\ub9c8\ud2b8 \uc54c\uace0\ub9ac\uc998\uc73c\ub85c \uac00\ub2a5\ud55c \uc801\uc740 \ud0a4\ud504\ub808\uc784\uc744 \uc720\uc9c0\ud558\uba70 \ud3b8\uc9d1\uc744 \uc6a9\uc774\ud558\uac8c \ud569\ub2c8\ub2e4.\", \"Activate the time remapping on the selected layers, adjusts the keyframes and adds a loop effect.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \uc2dc\uac04 \ub9ac\ub9f5\uc744 \ud65c\uc131\ud654\ud558\uace0, \ud0a4\ud504\ub808\uc784\uc744 \uc870\uc815\ud558\uba70 \ub8e8\ud504 \ud6a8\uacfc\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Creates a spatial effector to control properties.\n\nSelect the properties to control first, then click on this button.\": \"\uc18d\uc131\uc744 \uc81c\uc5b4\ud558\ub294 \uacf5\uac04 \uc774\ud399\ud130\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\n\n\uba3c\uc800 \uc81c\uc5b4\ud560 \uc18d\uc131\uc744 \uc120\ud0dd\ud55c \ud6c4 \uc774 \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uc138\uc694.\", \"Control properties using a map (texture) layer.\": \"\ub9f5(\ud14d\uc2a4\ucc98) \ub808\uc774\uc5b4\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc18d\uc131\uc744 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Add a random but smooth animation to the selected properties.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc5d0 \ub79c\ub364\ud558\uc9c0\ub9cc \ubd80\ub4dc\ub7ec\uc6b4 \uc560\ub2c8\uba54\uc774\uc158\uc744 \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Individual controls\": \"\uac1c\ubcc4 \uc81c\uc5b4\", \"Create one individual control for each property.\": \"\uac01 \uc18d\uc131\uc5d0 \ub300\ud574 \uac1c\ubcc4 \uc81c\uc5b4\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\", \"Unified control\": \"\ud1b5\ud569 \uc81c\uc5b4\", \"Create a single control for all properties.\na.k.a. The One Duik To Rule Them All.\": \"\ubaa8\ub4e0 \uc18d\uc131\uc744 \uc704\ud55c \ud558\ub098\uc758 \uc81c\uc5b4\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\n\uc77c\uba85 '\ubaa8\ub450\ub97c \uc9c0\ubc30\ud558\ub294 \ud558\ub098\uc758 Duik'.\", \"Add a control effect to randomize (and animate) the selected properties.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc744 \ub79c\ub364\ud654(\ubc0f \uc560\ub2c8\uba54\uc774\uc158)\ud558\ub294 \uc81c\uc5b4 \ud6a8\uacfc\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Creates a single control for all properties.\na.k.a. The One Duik To Rule Them All.\": \"\ubaa8\ub4e0 \uc18d\uc131\uc744 \uc704\ud55c \ud558\ub098\uc758 \uc81c\uc5b4\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\n\uc77c\uba85 '\ubaa8\ub450\ub97c \uc9c0\ubc30\ud558\ub294 \ud558\ub098\uc758 Duik'.\", \"Swing or blink.\nAlternate between two values, going back and forth,\nwith advanced interpolation options.\": \"\uc2a4\uc719 \ub610\ub294 \uae5c\ube61\uc784.\n\ub450 \uac12 \uc0ac\uc774\ub97c \uc655\ubcf5\ud558\uba70,\n\uace0\uae09 \ubcf4\uac04 \uc635\uc158\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.\", \"Swing\": \"\uc2a4\uc719\", \"Smoothly go back and forth between two values.\": \"\ub450 \uac12 \uc0ac\uc774\ub97c \ubd80\ub4dc\ub7fd\uac8c \uc655\ubcf5\ud569\ub2c8\ub2e4.\", \"Blink\": \"\uae5c\ube61\uc784\", \"Switch between two values, without interpolation.\": \"\ubcf4\uac04 \uc5c6\uc774 \ub450 \uac12 \uc0ac\uc774\ub97c \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Automates the rotation of the selected layers as wheels.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \ud68c\uc804\uc744 \ud720\ub85c \uc790\ub3d9\ud654\ud569\ub2c8\ub2e4.\", \"Add cycles to the animated properties,\n(with more features than the 'loopOut' and 'loopIn' expressions).\": \"\uc560\ub2c8\uba54\uc774\uc158 \uc18d\uc131\uc5d0 \uc0ac\uc774\ud074\uc744 \ucd94\uac00\ud569\ub2c8\ub2e4,\n('loopOut'\uacfc 'loopIn' \ud45c\ud604\uc2dd\ubcf4\ub2e4 \ub354 \ub9ce\uc740 \uae30\ub2a5 \uc81c\uacf5).\", \"Animate the selected character using a procedural walk or run cycle.\": \"\uc808\ucc28\uc801 \uac77\uae30 \ub610\ub294 \ub2ec\ub9ac\uae30 \uc0ac\uc774\ud074\ub85c \uc120\ud0dd\ud55c \uce90\ub9ad\ud130\uc5d0 \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc801\uc6a9\ud569\ub2c8\ub2e4.\", \"Neck\": \"\ubaa9\", \"Right hand\": \"\uc624\ub978\uc190\", \"Left hand\": \"\uc67c\uc190\", \"Right foot\": \"\uc624\ub978\ubc1c\", \"Left foot\": \"\uc67c\ubc1c\", \"Rig paint effects and brush strokes to animate them more easily.\": \"\ud398\uc778\ud2b8 \ud6a8\uacfc\uc640 \ube0c\ub7ec\uc2dc \uc2a4\ud2b8\ub85c\ud06c\ub97c \ub9ac\uae45\ud558\uc5ec \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc27d\uac8c \ub9cc\ub4ed\ub2c8\ub2e4.\", \"Bones\": \"\ubf08\ub300\", \"Select bones\": \"\ubf08\ub300 \uc120\ud0dd\", \"Select all bones\": \"\ubaa8\ub4e0 \ubf08\ub300 \uc120\ud0dd\", \"Show/hide bones\": \"\ubf08\ub300 \ud45c\uc2dc/\uc228\uae30\uae30\", \"Show/Hide all the bones.\n\n[Alt]: Only the unselected bones.\": \"\ubaa8\ub4e0 \ubf08\ub300\ub97c \ud45c\uc2dc/\uc228\uae41\ub2c8\ub2e4.\n\n[Alt]: \uc120\ud0dd\ub418\uc9c0 \uc54a\uc740 \ubf08\ub300\ub9cc.\", \"Duplicate bones\": \"\ubf08\ub300 \ubcf5\uc81c\", \"Duplicate the selected bones\": \"\uc120\ud0dd\ud55c \ubf08\ub300\ub97c \ubcf5\uc81c\ud569\ub2c8\ub2e4\", \"Automatically (try to) link the artwork layers to their corresponding bones.\": \"\uc544\ud2b8\uc6cc\ud06c \ub808\uc774\uc5b4\ub97c \ud574\ub2f9 \ubf08\ub300\uc5d0 \uc790\ub3d9\uc73c\ub85c \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"By default, bones and artworks are matched using the layer names.\": \"\uae30\ubcf8\uc801\uc73c\ub85c \ubf08\ub300\uc640 \uc544\ud2b8\uc6cc\ud06c\ub294 \ub808\uc774\uc5b4 \uc774\ub984\uc73c\ub85c \ub9e4\uce6d\ub429\ub2c8\ub2e4.\", \"[Alt]: Use the distance between the layers (using the anchor points of the layers) instead of their names.\": \"[Alt]: \uc774\ub984 \ub300\uc2e0 \ub808\uc774\uc5b4 \uac04 \uac70\ub9ac(\uc575\ucee4 \ud3ec\uc778\ud2b8 \uc0ac\uc6a9)\ub97c \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Edit mode\": \"\ud3b8\uc9d1 \ubaa8\ub4dc\", \"Bake bone appearances.\n\n[Alt]: Keep envelops.\n[Ctrl]: Keep deactivated noodles.\": \"\ubf08\ub300 \uc678\ud615\uc744 \ubca0\uc774\ud06c\ud569\ub2c8\ub2e4.\n\n[Alt]: \uc5d4\ubca8\ub85c\ud504 \uc720\uc9c0.\n[Ctrl]: \ube44\ud65c\uc131\ud654\ub41c \ub204\ub4e4 \uc720\uc9c0.\", \"Bone settings\": \"\ubf08\ub300 \uc124\uc815\", \"Edit selected bones\": \"\uc120\ud0dd\ud55c \ubf08\ub300 \ud3b8\uc9d1\", \"Current Selection\": \"\ud604\uc7ac \uc120\ud0dd\", \"Side\": \"\uce21\uba74\", \"Location\": \"\uc704\uce58\", \"Color\": \"\uc0c9\uc0c1\", \"Set the color of the selected layers.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Size\": \"\ud06c\uae30\", \"Change the size of the layer.\": \"\ub808\uc774\uc5b4\uc758 \ud06c\uae30\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Change the opacity of the bones.\": \"\ubf08\ub300\uc758 \ubd88\ud22c\uba85\ub3c4\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Group name\": \"\uadf8\ub8f9 \uc774\ub984\", \"Character / Group name\": \"\uce90\ub9ad\ud130 / \uadf8\ub8f9 \uc774\ub984\", \"Choose the name of the character.\": \"\uce90\ub9ad\ud130 \uc774\ub984\uc744 \uc120\ud0dd\ud558\uc138\uc694.\", \"Name\": \"\uc774\ub984\", \"(Limb) Name\": \"(\uc0ac\uc9c0) \uc774\ub984\", \"Change the name of the limb this layer belongs to\": \"\uc774 \ub808\uc774\uc5b4\uac00 \uc18d\ud55c \uc0ac\uc9c0\uc758 \uc774\ub984\uc744 \ubcc0\uacbd\ud569\ub2c8\ub2e4\", \"Envelop\": \"\uc5d4\ubca8\ub85c\ud504\", \"Enabled\": \"\ud65c\uc131\ud654\ub428\", \"Toggle the envelops of the selected bones\": \"\uc120\ud0dd\ud55c \ubf08\ub300\uc758 \uc5d4\ubca8\ub85c\ud504\ub97c \uc804\ud658\ud569\ub2c8\ub2e4\", \"Envelop opacity\": \"\uc5d4\ubca8\ub85c\ud504 \ubd88\ud22c\uba85\ub3c4\", \"Change the opacity of the envelop.\": \"\uc5d4\ubca8\ub85c\ud504\uc758 \ubd88\ud22c\uba85\ub3c4\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Envelop color\": \"\uc5d4\ubca8\ub85c\ud504 \uc0c9\uc0c1\", \"Set the color of the selected envelops.\": \"\uc120\ud0dd\ud55c \uc5d4\ubca8\ub85c\ud504\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Envelop stroke size\": \"\uc5d4\ubca8\ub85c\ud504 \uc120 \ud06c\uae30\", \"Change the size of the envelop stroke.\": \"\uc5d4\ubca8\ub85c\ud504 \uc120\uc758 \ud06c\uae30\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Envelop stroke color\": \"\uc5d4\ubca8\ub85c\ud504 \uc120 \uc0c9\uc0c1\", \"Set the color of the selected envelops strokes.\": \"\uc120\ud0dd\ud55c \uc5d4\ubca8\ub85c\ud504 \uc120\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Noodle\": \"\ub204\ub4e4\", \"Toggle the noodles of the selected bones\": \"\uc120\ud0dd\ud55c \ubf08\ub300\uc758 \ub204\ub4e4\uc744 \uc804\ud658\ud569\ub2c8\ub2e4\", \"Noodle color\": \"\ub204\ub4e4 \uc0c9\uc0c1\", \"Set the color of the selected noodles.\": \"\uc120\ud0dd\ud55c \ub204\ub4e4\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Pick selected layer\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4 \uac00\uc838\uc624\uae30\", \"Apply changes.\n\n[Alt]: assigns a random color to each bone.\": \"\ubcc0\uacbd \uc0ac\ud56d\uc744 \uc801\uc6a9\ud569\ub2c8\ub2e4.\n\n[Alt]: \uac01 \ubf08\ub300\uc5d0 \ub79c\ub364 \uc0c9\uc0c1\uc744 \uc9c0\uc815\ud569\ub2c8\ub2e4.\", \"Edit bones\": \"\ubf08\ub300 \ud3b8\uc9d1\", \"Character Name\": \"\uce90\ub9ad\ud130 \uc774\ub984\", \"Add an armature for an arm\": \"\ud314 \uc544\ub9c8\ucd94\uc5b4 \ucd94\uac00\", \"[Ctrl]: Auto-parent the selection to the new bones.\n[Alt]: Assign a random color to the new limb.\": \"[Ctrl]: \uc120\ud0dd \ud56d\ubaa9\uc744 \uc0c8 \ubf08\ub300\uc5d0 \uc790\ub3d9 \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\n[Alt]: \uc0c8 \uc0ac\uc9c0\uc5d0 \ub79c\ub364 \uc0c9\uc0c1\uc744 \uc9c0\uc815\ud569\ub2c8\ub2e4.\", \"Create\": \"\uc0dd\uc131\", \"Create arm\": \"\ud314 \uc0dd\uc131\", \"Forearm\": \"\uc804\uc644\", \"Add an armature for a leg\": \"\ub2e4\ub9ac \uc544\ub9c8\ucd94\uc5b4 \ucd94\uac00\", \"Create leg\": \"\ub2e4\ub9ac \uc0dd\uc131\", \"Thigh\": \"\ud5c8\ubc85\uc9c0\", \"Calf\": \"\uc885\uc544\ub9ac\", \"Toes\": \"\ubc1c\uac00\ub77d\", \"Add an armature for a spine (including the hips and head)\": \"\ucc99\ucd94 \uc544\ub9c8\ucd94\uc5b4 \ucd94\uac00 (\uc5c9\ub369\uc774\uc640 \uba38\ub9ac \ud3ec\ud568)\", \"Create spine\": \"\ucc99\ucd94 \uc0dd\uc131\", \"Add an armature for a hair strand.\": \"\uba38\ub9ac\uce74\ub77d \uac00\ub2e5 \uc544\ub9c8\ucd94\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Create hair strand\": \"\uba38\ub9ac\uce74\ub77d \uac00\ub2e5 \uc0dd\uc131\", \"Hair:\": \"\uba38\ub9ac\uce74\ub77d:\", \"layers\": \"\ub808\uc774\uc5b4\", \"Create tail\": \"\uaf2c\ub9ac \uc0dd\uc131\", \"Tail:\": \"\uaf2c\ub9ac:\", \"Add an armature for a wing.\": \"\ub0a0\uac1c \uc544\ub9c8\ucd94\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Create wing\": \"\ub0a0\uac1c \uc0dd\uc131\", \"Feathers:\": \"\uae43\ud138:\", \"Add an armature for the spine of a fish.\": \"\ubb3c\uace0\uae30 \ucc99\ucd94 \uc544\ub9c8\ucd94\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Create fish spine\": \"\ubb3c\uace0\uae30 \ucc99\ucd94 \uc0dd\uc131\", \"Spine:\": \"\ucc99\ucd94:\", \"Add an armature for a fin.\": \"\uc9c0\ub290\ub7ec\ubbf8 \uc544\ub9c8\ucd94\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Create fin\": \"\uc9c0\ub290\ub7ec\ubbf8 \uc0dd\uc131\", \"Fishbones:\": \"\uc0dd\uc120\ubf08:\", \"Hominoid\": \"\uc720\uc778\uc6d0\ud615\", \"Create limbs for an hominoid (Humans and apes).\": \"\uc720\uc778\uc6d0\ud615(\uc778\uac04\uacfc \uc720\uc778\uc6d0)\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Plantigrade\": \"\ubc1c\ubc14\ub2e5 \ubcf4\ud589\", \"Create limbs for a plantigrade (primates, bears, rabbits...).\": \"\ubc1c\ubc14\ub2e5 \ubcf4\ud589 \ub3d9\ubb3c(\uc601\uc7a5\ub958, \uacf0, \ud1a0\ub07c...)\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Digitigrade\": \"\ubc1c\uac00\ub77d \ubcf4\ud589\", \"Create limbs for a digitigrade (dogs, cats, dinosaurs...).\": \"\ubc1c\uac00\ub77d \ubcf4\ud589 \ub3d9\ubb3c(\uac1c, \uace0\uc591\uc774, \uacf5\ub8e1...)\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Ungulate\": \"\uc720\uc81c\ub958\", \"Create limbs for an ungulate (horses, cattle, giraffes, pigs, deers, camels, hippopotamuses...).\": \"\uc720\uc81c\ub958(\ub9d0, \uc18c, \uae30\ub9b0, \ub3fc\uc9c0, \uc0ac\uc2b4, \ub099\ud0c0, \ud558\ub9c8...)\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Arthropod\": \"\uc808\uc9c0\ub3d9\ubb3c\", \"Create limbs for an arthropod (insects, spiders, scorpions, crabs, shrimps...)\": \"\uc808\uc9c0\ub3d9\ubb3c(\uace4\ucda9, \uac70\ubbf8, \uc804\uac08, \uac8c, \uc0c8\uc6b0...)\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Bird\": \"\uc0c8\", \"Create limbs for a cute flying beast.\": \"\uadc0\uc5ec\uc6b4 \ub0a0\uc9d0\uc2b9\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Fish\": \"\ubb3c\uace0\uae30\", \"Create limbs for weird swimming beasts.\": \"\uc218\uc601\ud558\ub294 \ub3d9\ubb3c\uc758 \uc0ac\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Snake\": \"\ubc40\", \"Custom\": \"\ucee4\uc2a4\ud140\", \"Add a custom armature.\": \"\ucee4\uc2a4\ud140 \uc544\ub9c8\ucd94\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"[Ctrl]: Automatically parent the selected items (layers, path vertices or puppet pins) to the new bones.\": \"[Ctrl]: \uc120\ud0dd\ud55c \ud56d\ubaa9(\ub808\uc774\uc5b4, \ud328\uc2a4 \uaf2d\uc9d3\uc810 \ub610\ub294 \ud37c\ud3ab \ud540)\uc744 \uc0c8 \ubf08\ub300\uc5d0 \uc790\ub3d9 \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Create custom armature\": \"\ucee4\uc2a4\ud140 \uc544\ub9c8\ucd94\uc5b4 \uc0dd\uc131\", \"Number of bones:\": \"\ubf08\ub300 \uc218:\", \"Limb name\": \"\uc0ac\uc9c0 \uc774\ub984\", \"Cameras\": \"\uce74\uba54\ub77c\", \"Add guides in the composition to help the composition of the image.\nIncludes thirds, golden ratio, and other standard guides.\": \"\uc774\ubbf8\uc9c0 \uad6c\ub3c4\ub97c \ub3d5\uae30 \uc704\ud574 \uad6c\uc131\uc5d0 \uac00\uc774\ub4dc\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\n\uc0bc\ub4f1\ubd84, \ud669\uae08\ube44 \ub4f1 \ud45c\uc900 \uac00\uc774\ub4dc\ub97c \ud3ec\ud568\ud569\ub2c8\ub2e4.\", \"Adds an inverse constraint of the scale to the depth (Z position) of the 3D layers, so that their visual size doesn't change with their depth.\nWorks as a toggle: first click activates the effect, next click removes it from the selected layers.\": \"3D \ub808\uc774\uc5b4\uc758 \uae4a\uc774(Z \uc704\uce58)\uc5d0 \uc2a4\ucf00\uc77c\uc758 \uc5ed \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\n\ud1a0\uae00 \ubc29\uc2dd: \uccab \ud074\ub9ad\uc73c\ub85c \ud6a8\uacfc\ub97c \ud65c\uc131\ud654, \ub2e4\uc74c \ud074\ub9ad\uc73c\ub85c \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc5d0\uc11c \uc81c\uac70\ud569\ub2c8\ub2e4.\", \"There's no camera, please create a camera first.\": \"\uce74\uba54\ub77c\uac00 \uc5c6\uc2b5\ub2c8\ub2e4. \uba3c\uc800 \uce74\uba54\ub77c\ub97c \ub9cc\ub4e4\uc5b4\uc8fc\uc138\uc694.\", \"Nothing selected. Please select a layer first.\": \"\uc120\ud0dd\ub41c \ud56d\ubaa9\uc774 \uc5c6\uc2b5\ub2c8\ub2e4. \uba3c\uc800 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud558\uc138\uc694.\", \"Rig the selected camera to make it easier to animate.\nAlso includes nice behaviors like handheld camera or shoulder camera...\": \"\uc120\ud0dd\ud55c \uce74\uba54\ub77c\ub97c \ub9ac\uae45\ud558\uc5ec \uc560\ub2c8\uba54\uc774\uc158\uc744 \uc27d\uac8c \ub9cc\ub4ed\ub2c8\ub2e4.\n\ud578\ub4dc\ud5ec\ub4dc \uce74\uba54\ub77c, \uc204\ub354 \uce74\uba54\ub77c \uac19\uc740 \ud589\ub3d9\ub3c4 \ud3ec\ud568\ud569\ub2c8\ub2e4.\", \"There's no camera, or it's a one-node camera, but a two-node camera is needed.\nPlease, change to or create a two-node camera.\": \"\uce74\uba54\ub77c\uac00 \uc5c6\uac70\ub098 \ub2e8\uc77c \ub178\ub4dc \uce74\uba54\ub77c\uc785\ub2c8\ub2e4. 2-\ub178\ub4dc \uce74\uba54\ub77c\uac00 \ud544\uc694\ud569\ub2c8\ub2e4.\n2-\ub178\ub4dc \uce74\uba54\ub77c\ub85c \ubcc0\uacbd\ud558\uac70\ub098 \ub9cc\ub4e4\uc5b4\uc8fc\uc138\uc694.\", \"Create a fake camera, working with standard multiplane 2D Layers.\nParent the layers to the control null objects.\nDuplicate these control nulls if you need more levels.\nThis also includes nice behaviors like handheld camera or shoulder camera...\": \"\ud45c\uc900 \uba40\ud2f0\ud50c\ub808\uc778 2D \ub808\uc774\uc5b4\ub85c \uc791\ub3d9\ud558\ub294 \uac00\uc0c1 \uce74\uba54\ub77c\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\n\ub808\uc774\uc5b4\ub97c \uc81c\uc5b4 \ub110 \uc624\ube0c\uc81d\ud2b8\uc5d0 \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\n\ub354 \ub9ce\uc740 \ub808\ubca8\uc774 \ud544\uc694\ud558\uba74 \uc774 \uc81c\uc5b4 \ub110\uc744 \ubcf5\uc81c\ud558\uc138\uc694.\n\ud578\ub4dc\ud5ec\ub4dc \uce74\uba54\ub77c, \uc204\ub354 \uce74\uba54\ub77c \uac19\uc740 \ud589\ub3d9\ub3c4 \ud3ec\ud568\ud569\ub2c8\ub2e4.\", \"Command line\": \"\uba85\ub839\uc904\", \"Adjust other parameters for setting the size.\": \"\ud06c\uae30\ub97c \uc124\uc815\ud558\uae30 \uc704\ud55c \ub2e4\ub978 \ub9e4\uac1c\ubcc0\uc218\ub97c \uc870\uc815\ud569\ub2c8\ub2e4.\", \"Resize settings\": \"\ud06c\uae30 \uc870\uc815 \uc124\uc815\", \"Constraint the dimensions together.\": \"\uce58\uc218\ub97c \ud568\uaed8 \uc81c\ud55c\ud569\ub2c8\ub2e4.\", \"Width\": \"\ud3ed\", \"Height\": \"\ub192\uc774\", \"Pixel aspect\": \"\ud53d\uc140 \ube44\uc728\", \"Square Pixels\": \"\uc815\uc0ac\uac01\ud615 \ud53d\uc140\", \"D1/DV NTSC Widescreen (1.21)\": \"D1/DV NTSC \uc640\uc774\ub4dc\uc2a4\ud06c\ub9b0 (1.21)\", \"D1/DV PAL Widescreen (1.46)\": \"D1/DV PAL \uc640\uc774\ub4dc\uc2a4\ud06c\ub9b0 (1.46)\", \"Anamorphic 2:1 (2)\": \"\uc544\ub098\ubaa8\ud53d 2:1 (2)\", \"Frame rate\": \"\ud504\ub808\uc784 \ub808\uc774\ud2b8\", \"Display\": \"\ub514\uc2a4\ud50c\ub808\uc774\", \"Resolution\": \"\ud574\uc0c1\ub3c4\", \"Preserve resolution\": \"\ud574\uc0c1\ub3c4 \uc720\uc9c0\", \"Preserve\": \"\uc720\uc9c0\", \"Background color\": \"\ubc30\uacbd\uc0c9\", \"Shy layers\": \"\uc228\uae40 \ub808\uc774\uc5b4\", \"Hide\": \"\uc228\uae30\uae30\", \"Rendering\": \"\ub80c\ub354\ub9c1\", \"Proxy\": \"\ud504\ub85d\uc2dc\", \"Renderer\": \"\ub80c\ub354\ub7ec\", \"Preserve frame rate\": \"\ud504\ub808\uc784 \ub808\uc774\ud2b8 \uc720\uc9c0\", \"Frame blending\": \"\ud504\ub808\uc784 \ube14\ub80c\ub529\", \"Motion blur\": \"\ubaa8\uc158 \ube14\ub7ec\", \"Shutter angle\": \"\uc154\ud130 \uac01\ub3c4\", \"Shutter phase\": \"\uc154\ud130 \ud398\uc774\uc988\", \"Samples\": \"\uc0d8\ud50c\", \"Adaptive sample limit\": \"\uc801\uc751\ud615 \uc0d8\ud50c \uc81c\ud55c\", \"Update precompositions\": \"\ud504\ub9ac\ucef4\ud504 \uc5c5\ub370\uc774\ud2b8\", \"Comp settings\": \"\ucef4\ud504 \uc124\uc815\", \"Set the current or selected composition(s) settings, also changing the settings of all precompositions.\": \"\ud604\uc7ac \ub610\ub294 \uc120\ud0dd\ud55c \ucef4\ud3ec\uc9c0\uc158 \uc124\uc815\uc744 \uc124\uc815\ud558\uba70, \ubaa8\ub4e0 \ud504\ub9ac\ucef4\ud504 \uc124\uc815\ub3c4 \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Links and constraints\": \"\ub9c1\ud06c \ubc0f \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Lock prop.\": \"\uc18d\uc131 \uc7a0\uae08\", \"Lock the value of the selected properties.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc758 \uac12\uc744 \uc7a0\uae09\ub2c8\ub2e4.\", \"Zero out the selected layers transformation.\n[Alt]: Reset the transformation of the selected layers to 0.\n[Ctrl] + [Alt]: Also reset the opacity to 100 %.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \ubcc0\ud658\uc744 \uae30\ubcf8\uac12\uc73c\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\n[Alt]: \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \ubcc0\ud658\uc744 0\uc73c\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\n[Ctrl] + [Alt]: \ubd88\ud22c\uba85\ub3c4\ub3c4 100%\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\", \"Expose the transformation of layers using a nice controller.\": \"\ud3b8\ub9ac\ud55c \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0ac\uc6a9\ud558\uc5ec \ub808\uc774\uc5b4 \ubcc0\ud658\uc744 \ub178\ucd9c\ud569\ub2c8\ub2e4.\", \"Create locator.\": \"\ub85c\ucf00\uc774\ud130 \uc0dd\uc131.\", \"Extract locators.\": \"\ub85c\ucf00\uc774\ud130 \ucd94\ucd9c.\", \"Use expressions\": \"\ud45c\ud604\uc2dd \uc0ac\uc6a9\", \"Use essential properties\": \"\ud544\uc218 \uc18d\uc131 \uc0ac\uc6a9\", \"Measure distance\": \"\uac70\ub9ac \uce21\uc815\", \"Measure the distance between two layers.\": \"\ub450 \ub808\uc774\uc5b4 \uac04\uc758 \uac70\ub9ac\ub97c \uce21\uc815\ud569\ub2c8\ub2e4.\", \"Select two layers to measure the distance between them.\": \"\ub450 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud558\uc5ec \uac70\ub9ac\ub97c \uce21\uc815\ud569\ub2c8\ub2e4.\", \"The distance is %1 px\": \"\uac70\ub9ac\ub294 %1 px\uc785\ub2c8\ub2e4\", \"Prop. info\": \"\uc18d\uc131 \uc815\ubcf4\", \"Get property detailed information.\": \"\uc18d\uc131 \uc0c1\uc138 \uc815\ubcf4\ub97c \uac00\uc838\uc635\ub2c8\ub2e4.\", \"Get property info\": \"\uc18d\uc131 \uc815\ubcf4 \uac00\uc838\uc624\uae30\", \"Connect slave properties to a master property.\": \"\uc885\uc18d \uc18d\uc131\uc744 \ub9c8\uc2a4\ud130 \uc18d\uc131\uc5d0 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Layer opacities\": \"\ub808\uc774\uc5b4 \ubd88\ud22c\uba85\ub3c4\", \"Connects the selected layers to the control you've just set, using their opacity properties to switch their visibility.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\ub97c \ubc29\uae08 \uc124\uc815\ud55c \ucee8\ud2b8\ub864\uc5d0 \uc5f0\uacb0\ud558\uc5ec, \ubd88\ud22c\uba85\ub3c4 \uc18d\uc131\uc73c\ub85c \uac00\uc2dc\uc131\uc744 \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Properties\": \"\uc18d\uc131\", \"Connects the selected properties to the control you've just set.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc744 \ubc29\uae08 \uc124\uc815\ud55c \ucee8\ud2b8\ub864\uc5d0 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Connects the selected keys to the control you've just set.\": \"\uc120\ud0dd\ud55c \ud0a4\ub97c \ubc29\uae08 \uc124\uc815\ud55c \ucee8\ud2b8\ub864\uc5d0 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"2D Slider\": \"2D \uc2ac\ub77c\uc774\ub354\", \"Angle\": \"\uac01\ub3c4\", \"Axis\": \"\ucd95\", \"Channel\": \"\ucc44\ub110\", \"Choose or create control\": \"\ucee8\ud2b8\ub864 \uc120\ud0dd \ub610\ub294 \uc0dd\uc131\", \"Create a slider controller.\": \"\uc2ac\ub77c\uc774\ub354 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a 2D slider controller.\": \"2D \uc2ac\ub77c\uc774\ub354 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an angle controller.\": \"\uac01\ub3c4 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a controller to measure lengths, angles and other coordinates between layers.\": \"\ub808\uc774\uc5b4 \uac04 \uae38\uc774, \uac01\ub3c4 \ubc0f \uae30\ud0c0 \uc88c\ud45c\ub97c \uce21\uc815\ud558\ub294 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Layer list\": \"\ub808\uc774\uc5b4 \ubaa9\ub85d\", \"Creates a dropdown control to control the visibility of a list of layers.\n\nFirst, select the layer where to create the controlling effect, then click the button to get to the next step.\": \"\ub808\uc774\uc5b4 \ubaa9\ub85d\uc758 \uac00\uc2dc\uc131\uc744 \uc81c\uc5b4\ud558\ub294 \ub4dc\ub86d\ub2e4\uc6b4 \ucee8\ud2b8\ub864\uc744 \ub9cc\ub4ed\ub2c8\ub2e4.\n\n\uba3c\uc800 \uc81c\uc5b4 \ud6a8\uacfc\ub97c \ub9cc\ub4e4 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud55c \ud6c4 \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uc138\uc694.\", \"Nothing selected. Please select some layers first.\": \"\uc120\ud0dd\ub41c \ud56d\ubaa9\uc774 \uc5c6\uc2b5\ub2c8\ub2e4. \uba3c\uc800 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud558\uc138\uc694.\", \"Create a spatial effector to control properties.\n\nSelect the properties to control first, then click on this button.\": \"\uc18d\uc131\uc744 \uc81c\uc5b4\ud558\ub294 \uacf5\uac04 \uc774\ud399\ud130\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\n\n\uba3c\uc800 \uc81c\uc5b4\ud560 \uc18d\uc131\uc744 \uc120\ud0dd\ud55c \ud6c4 \uc774 \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uc138\uc694.\", \"Pick texture\": \"\ud14d\uc2a4\ucc98 \uc120\ud0dd\", \"Choose a layer to use as a texture map to control the properties.\": \"\uc18d\uc131\uc744 \uc81c\uc5b4\ud558\uae30 \uc704\ud55c \ud14d\uc2a4\ucc98 \ub9f5\uc73c\ub85c \uc0ac\uc6a9\ud560 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Pick audio\": \"\uc624\ub514\uc624 \uc120\ud0dd\", \"Controls properties using an audio layer.\": \"\uc624\ub514\uc624 \ub808\uc774\uc5b4\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc18d\uc131\uc744 \uc81c\uc5b4\ud569\ub2c8\ub2e4.\", \"Pick control\": \"\ucee8\ud2b8\ub864 \uc120\ud0dd\", \"Uses the selected property, layer or already existing control.\": \"\uc120\ud0dd\ud55c \uc18d\uc131, \ub808\uc774\uc5b4 \ub610\ub294 \uae30\uc874 \ucee8\ud2b8\ub864\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Connecting\": \"\uc5f0\uacb0 \uc911\", \"Value\": \"\uac12\", \"Speed\": \"\uc18d\ub3c4\", \"Velocity\": \"\uc18d\ub3c4\", \"Select the child properties or layers,\nand connect them.\": \"\ud558\uc704 \uc18d\uc131 \ub610\ub294 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud558\uace0\n\uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Connecting dropdown menu to layers:\n\nSelect the child layers then connect.\": \"\ub4dc\ub86d\ub2e4\uc6b4 \uba54\ub274\ub97c \ub808\uc774\uc5b4\uc5d0 \uc5f0\uacb0:\n\n\ud558\uc704 \ub808\uc774\uc5b4\ub97c \uc120\ud0dd\ud55c \ud6c4 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Connect layer opacities\": \"\ub808\uc774\uc5b4 \ubd88\ud22c\uba85\ub3c4 \uc5f0\uacb0\", \"Connect the selected layers to the control you've just set, using their opacity properties to switch their visibility.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\ub97c \ubc29\uae08 \uc124\uc815\ud55c \ucee8\ud2b8\ub864\uc5d0 \uc5f0\uacb0\ud558\uc5ec, \ubd88\ud22c\uba85\ub3c4 \uc18d\uc131\uc73c\ub85c \uac00\uc2dc\uc131\uc744 \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Morph selected properties between arbitrary keyframes.\": \"\uc784\uc758\uc758 \ud0a4\ud504\ub808\uc784 \uac04 \uc120\ud0dd\ud55c \uc18d\uc131\uc744 \ubaa8\ud551\ud569\ub2c8\ub2e4.\", \"Add pin layers to control spatial properties and B\u00e9zier paths.\n[Alt]: Also create tangents for B\u00e9zier path properties.\": \"\uacf5\uac04 \uc18d\uc131\uacfc \ubca0\uc9c0\uc5b4 \ud328\uc2a4\ub97c \uc81c\uc5b4\ud558\ub294 \ud540 \ub808\uc774\uc5b4\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\n[Alt]: \ubca0\uc9c0\uc5b4 \ud328\uc2a4 \uc18d\uc131\uc758 \ud0c4\uc820\ud2b8\ub3c4 \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Change the opacity of the pins.\": \"\ud540\uc758 \ubd88\ud22c\uba85\ub3c4\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Change the name of the limb this layer belongs to.\": \"\uc774 \ub808\uc774\uc5b4\uac00 \uc18d\ud55c \uc0ac\uc9c0\uc758 \uc774\ub984\uc744 \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Edit pins\": \"\ud540 \ud3b8\uc9d1\", \"Kinematics\": \"\ud0a4\ub124\ub9c8\ud2f1\uc2a4\", \"Create Inverse and Forward Kinematics.\": \"\uc5ed\uc6b4\ub3d9\ud559(IK)\uacfc \uc21c\uc6b4\ub3d9\ud559(FK)\uc744 \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Standard Inverse Kinematics.\": \"\ud45c\uc900 \uc5ed\uc6b4\ub3d9\ud559(IK).\", \"1+2-layer IK\": \"1+2 \ub808\uc774\uc5b4 IK\", \"Create a one-layer IK combined with a two-layer IK\nto handle Z-shape limbs.\": \"Z\ud615 \uc0ac\uc9c0\ub97c \ucc98\ub9ac\ud558\uae30 \uc704\ud574 1\ub808\uc774\uc5b4 IK\uc640 2\ub808\uc774\uc5b4 IK\ub97c \uc870\ud569\ud569\ub2c8\ub2e4.\", \"2+1-layer IK\": \"2+1 \ub808\uc774\uc5b4 IK\", \"Create a two-layer IK combined with a one-layer IK\nto handle Z-shape limbs.\": \"Z\ud615 \uc0ac\uc9c0\ub97c \ucc98\ub9ac\ud558\uae30 \uc704\ud574 2\ub808\uc774\uc5b4 IK\uc640 1\ub808\uc774\uc5b4 IK\ub97c \uc870\ud569\ud569\ub2c8\ub2e4.\", \"B\u00e9zier Inverse Kinematics.\": \"\ubca0\uc9c0\uc5b4 \uc5ed\uc6b4\ub3d9\ud559(IK).\", \"Forward Kinematics\nwith automatic overlap and follow-through.\": \"\uc790\ub3d9 \uc624\ubc84\ub7a9\uacfc \ud314\ub85c\uc2a4\ub8e8\uac00 \uc788\ub294 \uc21c\uc6b4\ub3d9\ud559(FK).\", \"Parent\": \"\ubd80\ubaa8\", \"Create parent constraints.\": \"\ubd80\ubaa8 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Parent all the selected layers to the last selected one.\n\n[Alt]: Parent only the orphans.\nOr:\n[Ctrl]: Parent layers as a chain, from ancestor to child, in the order of the selection.\": \"\ubaa8\ub4e0 \uc120\ud0dd \ub808\uc774\uc5b4\ub97c \ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc5d0 \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\n\n[Alt]: \uace0\uc544 \ub808\uc774\uc5b4\ub9cc \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\n\ub610\ub294:\n[Ctrl]: \uc120\ud0dd \uc21c\uc11c\ub300\ub85c \uc870\uc0c1\uc5d0\uc11c \uc790\uc2dd\uc73c\ub85c \uccb4\uc778 \ud615\ud0dc\ub85c \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Animatable parent.\": \"\uc560\ub2c8\uba54\uc774\uc158 \uac00\ub2a5\ud55c \ubd80\ubaa8.\", \"Parent across comps...\": \"\ucef4\ud504 \uac04 \ubd80\ubaa8 \uc5f0\uacb0...\", \"Parent layers across compositions.\": \"\ud569\uc131 \uac04 \ub808\uc774\uc5b4\ub97c \ubd80\ubaa8 \uc5f0\uacb0\ud569\ub2c8\ub2e4.\", \"Parent comp\": \"\ubd80\ubaa8 \ucef4\ud504\", \"Parent layer\": \"\ubd80\ubaa8 \ub808\uc774\uc5b4\", \"Create transform constraints (position, orientation, path...).\": \"\ubcc0\ud658 \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8(\uc704\uce58, \ubc29\ud5a5, \ud328\uc2a4...)\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Constraint the location of a layer to the position of other layers.\": \"\ub808\uc774\uc5b4\uc758 \uc704\uce58\ub97c \ub2e4\ub978 \ub808\uc774\uc5b4\uc758 \uc704\uce58\ub85c \uc81c\ud55c\ud569\ub2c8\ub2e4.\", \"Constraint the orientation of a layer to the orientation of other layers.\": \"\ub808\uc774\uc5b4\uc758 \ubc29\ud5a5\uc744 \ub2e4\ub978 \ub808\uc774\uc5b4\uc758 \ubc29\ud5a5\uc73c\ub85c \uc81c\ud55c\ud569\ub2c8\ub2e4.\", \"Constraint the location and orientation of a layer to a B\u00e9zier path.\n\n\": \"\ub808\uc774\uc5b4\uc758 \uc704\uce58\uc640 \ubc29\ud5a5\uc744 \ubca0\uc9c0\uc5b4 \ud328\uc2a4\ub85c \uc81c\ud55c\ud569\ub2c8\ub2e4.\n\n\", \"Pick path...\": \"\ud328\uc2a4 \uc120\ud0dd...\", \"Select controllers\": \"\ucee8\ud2b8\ub864\ub7ec \uc120\ud0dd\", \"Select all controllers\": \"\ubaa8\ub4e0 \ucee8\ud2b8\ub864\ub7ec \uc120\ud0dd\", \"Show/hide ctrls\": \"\ucee8\ud2b8\ub864\ub7ec \ud45c\uc2dc/\uc228\uae30\uae30\", \"Show/Hide controllers\n\n[Alt]: Only the unselected controllers.\": \"\ucee8\ud2b8\ub864\ub7ec\ub97c \ud45c\uc2dc/\uc228\uae41\ub2c8\ub2e4.\n\n[Alt]: \uc120\ud0dd\ub418\uc9c0 \uc54a\uc740 \ucee8\ud2b8\ub864\ub7ec\ub9cc.\", \"Tag as controllers\": \"\ucee8\ud2b8\ub864\ub7ec\ub85c \ud0dc\uadf8\", \"Bake controllers appearance\": \"\ucee8\ud2b8\ub864\ub7ec \uc678\ud615 \ubca0\uc774\ud06c\", \"Controller settings\": \"\ucee8\ud2b8\ub864\ub7ec \uc124\uc815\", \"Edit selected controllers.\": \"\uc120\ud0dd\ud55c \ucee8\ud2b8\ub864\ub7ec\ub97c \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\", \"Use AE Null Objects\": \"AE \ub110 \uc624\ube0c\uc81d\ud2b8 \uc0ac\uc6a9\", \"Use Shape Layers\": \"\uc170\uc774\ud504 \ub808\uc774\uc5b4 \uc0ac\uc6a9\", \"Use Shape Layers (Draft mode)\": \"\uc170\uc774\ud504 \ub808\uc774\uc5b4 \uc0ac\uc6a9 (\ucd08\uc548 \ubaa8\ub4dc)\", \"Use Raster Layers (PNG)\": \"\ub798\uc2a4\ud130 \ub808\uc774\uc5b4 (PNG) \uc0ac\uc6a9\", \"Don't lock scale of null controllers.\": \"\ub110 \ucee8\ud2b8\ub864\ub7ec\uc758 \uc2a4\ucf00\uc77c\uc744 \uc7a0\uadf8\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\", \"The scale of null controllers, and After Effects nulls as controllers created by Duik won't be locked by default.\": \"\ub110 \ucee8\ud2b8\ub864\ub7ec\uc758 \uc2a4\ucf00\uc77c\uacfc Duik\uc774 \ub9cc\ub4e0 After Effects \ub110 \ucee8\ud2b8\ub864\ub7ec\ub294 \uae30\ubcf8\uc801\uc73c\ub85c \uc7a0\uae30\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\", \"Icon color\": \"\uc544\uc774\ucf58 \uc0c9\uc0c1\", \"Set the color of the selected controllers.\n\n[Alt]: assigns a random color for each controller.\": \"\uc120\ud0dd\ud55c \ucee8\ud2b8\ub864\ub7ec\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\n\n[Alt]: \uac01 \ucee8\ud2b8\ub864\ub7ec\uc5d0 \ub79c\ub364 \uc0c9\uc0c1\uc744 \uc9c0\uc815\ud569\ub2c8\ub2e4.\", \"Icon size\": \"\uc544\uc774\ucf58 \ud06c\uae30\", \"Change the size of the controller.\": \"\ucee8\ud2b8\ub864\ub7ec\uc758 \ud06c\uae30\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Icon opacity\": \"\uc544\uc774\ucf58 \ubd88\ud22c\uba85\ub3c4\", \"Change the opacity of the controllers.\": \"\ucee8\ud2b8\ub864\ub7ec\uc758 \ubd88\ud22c\uba85\ub3c4\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Anchor color\": \"\uc575\ucee4 \uc0c9\uc0c1\", \"Set the color of the selected anchors.\n\n[Alt]: assigns a random color for each anchor.\": \"\uc120\ud0dd\ud55c \uc575\ucee4\uc758 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\n\n[Alt]: \uac01 \uc575\ucee4\uc5d0 \ub79c\ub364 \uc0c9\uc0c1\uc744 \uc9c0\uc815\ud569\ub2c8\ub2e4.\", \"Anchor size\": \"\uc575\ucee4 \ud06c\uae30\", \"Change the size of the anchor.\": \"\uc575\ucee4\uc758 \ud06c\uae30\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Anchor opacity\": \"\uc575\ucee4 \ubd88\ud22c\uba85\ub3c4\", \"Change the opacity of the anchors.\": \"\uc575\ucee4\uc758 \ubd88\ud22c\uba85\ub3c4\ub97c \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Apply changes.\n\n[Alt]: assigns a random color to each layer.\": \"\ubcc0\uacbd \uc0ac\ud56d\uc744 \uc801\uc6a9\ud569\ub2c8\ub2e4.\n\n[Alt]: \uac01 \ub808\uc774\uc5b4\uc5d0 \ub79c\ub364 \uc0c9\uc0c1\uc744 \uc9c0\uc815\ud569\ub2c8\ub2e4.\", \"Edit Controllers\": \"\ucee8\ud2b8\ub864\ub7ec \ud3b8\uc9d1\", \"Create a rotation controller.\": \"\ud68c\uc804 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a horizontal translation controller.\": \"\uc218\ud3c9 \uc774\ub3d9 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a vertical translation controller.\": \"\uc218\uc9c1 \uc774\ub3d9 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a translation controller.\": \"\uc774\ub3d9 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a translation and rotation controller.\": \"\uc774\ub3d9 \ubc0f \ud68c\uc804 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a camera controller.\": \"\uce74\uba54\ub77c \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Creates a slider controller.\": \"\uc2ac\ub77c\uc774\ub354 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an eyebrow controller.\": \"\ub208\uc379 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Creates an ear controller.\": \"\uadc0 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a hair controller.\": \"\uba38\ub9ac\uce74\ub77d \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a mouth controller.\": \"\uc785 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a nose controller.\": \"\ucf54 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an eye controller.\": \"\ub208 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a head controller.\": \"\uba38\ub9ac \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a foot controller.\": \"\ubc1c \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a paw controller.\": \"\ubc1c\ubc14\ub2e5 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a hoof controller.\": \"\ubc1c\uad7d \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a hand controller.\": \"\uc190 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a hips controller.\": \"\uc5c9\ub369\uc774 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a body controller.\": \"\ubab8\uccb4 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a neck and shoulders controller.\": \"\ubaa9\uacfc \uc5b4\uae68 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a torso controller.\": \"\ubab8\ud1b5 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a vertebrae (neck or spine) controller.\": \"\ucc99\ucd94\ubf08(\ubaa9 \ub610\ub294 \ucc99\ucd94) \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a fin controller.\": \"\uc9c0\ub290\ub7ec\ubbf8 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a wing controller.\": \"\ub0a0\uac1c \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a pincer controller.\": \"\uc9d1\uac8c \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a tail controller.\": \"\uaf2c\ub9ac \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a hair strand controller.\": \"\uba38\ub9ac\uce74\ub77d \uac00\ub2e5 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an audio controller.\": \"\uc624\ub514\uc624 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a null controller.\": \"\ub110 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an After Effects null controller.\": \"After Effects \ub110 \ucee8\ud2b8\ub864\ub7ec\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Pseudo-effects\": \"\uc218\ub3c4 \uc774\ud399\ud2b8\", \"Create pre-rigged pseudo-effects.\": \"\uc0ac\uc804 \ub9ac\uae45\ub41c \uc218\ub3c4 \uc774\ud399\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Eyes\": \"\ub208\", \"Create a pre-rigged pseudo-effect to control eyes.\": \"\ub208\uc744 \uc81c\uc5b4\ud558\ub294 \uc0ac\uc804 \ub9ac\uae45\ub41c \uc218\ub3c4 \uc774\ud399\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Fingers\": \"\uc190\uac00\ub77d\", \"Create a pre-rigged pseudo-effect to control fingers.\": \"\uc190\uac00\ub77d\uc744 \uc81c\uc5b4\ud558\ub294 \uc0ac\uc804 \ub9ac\uae45\ub41c \uc218\ub3c4 \uc774\ud399\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a pre-rigged pseudo-effect to control a hand and its fingers.\": \"\uc190\uacfc \uc190\uac00\ub77d\uc744 \uc81c\uc5b4\ud558\ub294 \uc0ac\uc804 \ub9ac\uae45\ub41c \uc218\ub3c4 \uc774\ud399\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a pre-rigged pseudo-effect to control a head.\": \"\uba38\ub9ac\ub97c \uc81c\uc5b4\ud558\ub294 \uc0ac\uc804 \ub9ac\uae45\ub41c \uc218\ub3c4 \uc774\ud399\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Extract\": \"\ucd94\ucd9c\", \"Extract the controllers from the selected precomposition, to animate from outside the composition.\": \"\uc120\ud0dd\ud55c \ud504\ub9ac\ucef4\ud504\uc5d0\uc11c \ucee8\ud2b8\ub864\ub7ec\ub97c \ucd94\ucd9c\ud558\uc5ec, \ucef4\ud504 \uc678\ubd80\uc5d0\uc11c \uc560\ub2c8\uba54\uc774\uc158\ud569\ub2c8\ub2e4.\", \"OCO Meta-rig\": \"OCO \uba54\ud0c0\ub9ac\uadf8\", \"Create, import, export Meta-Rigs and templates\": \"\uba54\ud0c0\ub9ac\uadf8\uc640 \ud15c\ud50c\ub9bf\uc744 \uc0dd\uc131, \uac00\uc838\uc624\uae30, \ub0b4\ubcf4\ub0b4\uae30\ud569\ub2c8\ub2e4\", \"The bones and armatures panel\": \"\ubf08\ub300 \ubc0f \uc544\ub9c8\ucd94\uc5b4 \ud328\ub110\", \"Links & constraints\": \"\ub9c1\ud06c & \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8\", \"Automation & expressions\": \"\uc790\ub3d9\ud654 & \ud45c\ud604\uc2dd\", \"Tools\": \"\ub3c4\uad6c\", \"Get help\": \"\ub3c4\uc6c0\ub9d0\", \"Read the doc!\": \"\ubb38\uc11c\ub97c \uc77d\uc73c\uc138\uc694!\", \"Support %1 if you love it!\": \"%1\uc774 \ub9c8\uc74c\uc5d0 \ub4dc\uc2dc\uba74 \uc9c0\uc6d0\ud574\uc8fc\uc138\uc694!\", \"Create a null object.\": \"\ub110 \uc624\ube0c\uc81d\ud2b8\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a solid layer.\": \"\uc194\ub9ac\ub4dc \ub808\uc774\uc5b4\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create an adjustment layer.\": \"\uc870\uc815 \ub808\uc774\uc5b4\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Create a shape layer.\": \"\uc170\uc774\ud504 \ub808\uc774\uc5b4\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Circle\": \"\uc6d0\", \"Square\": \"\uc0ac\uac01\ud615\", \"Rounded square\": \"\ub465\uadfc \uc0ac\uac01\ud615\", \"Polygon\": \"\ub2e4\uac01\ud615\", \"Star\": \"\ubcc4\", \"Zero out the selected layers transformation.\n[Alt]: Reset the transformation of the selected layers to 0.\n[Ctrl] + [Alt]: Also resets the opacity to 100 %.\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \ubcc0\ud658\uc744 \uae30\ubcf8\uac12\uc73c\ub85c \uc124\uc815\ud569\ub2c8\ub2e4.\n[Alt]: \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc758 \ubcc0\ud658\uc744 0\uc73c\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\n[Ctrl] + [Alt]: \ubd88\ud22c\uba85\ub3c4\ub3c4 100%\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\", \"Auto-Rename & Tag\": \"\uc790\ub3d9 \uc774\ub984\ubcc0\uacbd & \ud0dc\uadf8\", \"Automagically renames, tags and groups the selected layers (or all of them if there's no selection)\": \"\uc120\ud0dd\ud55c \ub808\uc774\uc5b4(\uc120\ud0dd \uc5c6\uc73c\uba74 \ubaa8\ub4e0 \ub808\uc774\uc5b4)\uc758 \uc774\ub984\uc744 \uc790\ub3d9\uc73c\ub85c \ubcc0\uacbd, \ud0dc\uadf8 \ubc0f \uadf8\ub8f9\ud654\ud569\ub2c8\ub2e4.\", \"Layer manager\": \"\ub808\uc774\uc5b4 \uad00\ub9ac\uc790\", \"Setting layers type...\": \"\ub808\uc774\uc5b4 \uc720\ud615 \uc124\uc815 \uc911...\", \"Setting layers location...\": \"\ub808\uc774\uc5b4 \uc704\uce58 \uc124\uc815 \uc911...\", \"Setting layers side...\": \"\ub808\uc774\uc5b4 \uce21\uba74 \uc124\uc815 \uc911...\", \"Setting layers group name...\": \"\ub808\uc774\uc5b4 \uadf8\ub8f9 \uc774\ub984 \uc124\uc815 \uc911...\", \"Setting layers name...\": \"\ub808\uc774\uc5b4 \uc774\ub984 \uc124\uc815 \uc911...\", \"Create, import, export Meta-Rigs and templates\n\n\": \"\uba54\ud0c0\ub9ac\uadf8\uc640 \ud15c\ud50c\ub9bf\uc744 \uc0dd\uc131, \uac00\uc838\uc624\uae30, \ub0b4\ubcf4\ub0b4\uae30\ud569\ub2c8\ub2e4.\n\n\", \"[Alt]: Launches the corresponding ScriptUI Stand-Alone panel if it is installed.\": \"[Alt]: \uc124\uce58\ub41c \uacbd\uc6b0 \ud574\ub2f9 ScriptUI \ub3c5\ub9bd \ud328\ub110\uc744 \uc2e4\ud589\ud569\ub2c8\ub2e4.\", \"Test failed!\": \"\ud14c\uc2a4\ud2b8 \uc2e4\ud328!\", \"Keep this panel open\": \"\uc774 \ud328\ub110 \uc5f4\uc5b4\ub450\uae30\", \"%1 Settings\": \"%1 \uc124\uc815\", \"Set the language of the interface.\": \"\uc778\ud130\ud398\uc774\uc2a4 \uc5b8\uc5b4\ub97c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Settings file\": \"\uc124\uc815 \ud30c\uc77c\", \"Set the location of the settings file.\": \"\uc124\uc815 \ud30c\uc77c\uc758 \uc704\uce58\ub97c \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Set the highlight color.\": \"\uac15\uc870 \uc0c9\uc0c1\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"After Effects Blue\": \"After Effects \ud30c\ub780\uc0c9\", \"The After Effects highlighting blue\": \"After Effects \uac15\uc870 \ud30c\ub780\uc0c9\", \"RxLab Purple\": \"RxLab \ubcf4\ub77c\uc0c9\", \"The RxLaboratory Purple\": \"RxLaboratory \ubcf4\ub77c\uc0c9\", \"Rainbox Red\": \"Rainbox \ube68\uac04\uc0c9\", \"The Rainbox Productions Red\": \"Rainbox Productions \ube68\uac04\uc0c9\", \"After Effects Orange (CS6)\": \"After Effects \uc8fc\ud669\uc0c9 (CS6)\", \"The After Effects highlighting orange from good ol'CS6\": \"\uadf8 \uc2dc\uc808 CS6\uc758 After Effects \uac15\uc870 \uc8fc\ud669\uc0c9\", \"Custom...\": \"\uc0ac\uc6a9\uc790 \uc9c0\uc815...\", \"Select a custom color.\": \"\uc0ac\uc6a9\uc790 \uc9c0\uc815 \uc0c9\uc0c1\uc744 \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Select the UI mode.\": \"UI \ubaa8\ub4dc\ub97c \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Rookie\": \"\ucd08\ubcf4\", \"The easiest-to-use mode, but also the biggest UI.\": \"\uac00\uc7a5 \uc0ac\uc6a9\ud558\uae30 \uc26c\uc6b4 \ubaa8\ub4dc\uc774\uc9c0\ub9cc UI\uac00 \uac00\uc7a5 \ud07d\ub2c8\ub2e4.\", \"Standard\": \"\ud45c\uc900\", \"The standard not-too-big UI.\": \"\uc801\ub2f9\ud55c \ud06c\uae30\uc758 \ud45c\uc900 UI.\", \"Expert\": \"\uc804\ubb38\uac00\", \"The smallest UI, for expert users.\": \"\uc804\ubb38\uac00 \uc0ac\uc6a9\uc790\ub97c \uc704\ud55c \uac00\uc7a5 \uc791\uc740 UI.\", \"Extract UI Icons\": \"UI \uc544\uc774\ucf58 \ucd94\ucd9c\", \"You can replace existing icons when they're extracted.\": \"\ucd94\ucd9c\ud560 \ub54c \uae30\uc874 \uc544\uc774\ucf58\uc744 \uad50\uccb4\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Open the folder containing the extracted icons.\": \"\ucd94\ucd9c\ub41c \uc544\uc774\ucf58\uc774 \uc788\ub294 \ud3f4\ub354\ub97c \uc5fd\ub2c8\ub2e4.\", \"Normal mode\": \"\uc77c\ubc18 \ubaa8\ub4dc\", \"Use at your own risk!\": \"\uc0ac\uc6a9\uc5d0 \ub530\ub978 \uc704\ud5d8\uc740 \ubcf8\uc778 \ubd80\ub2f4!\", \"Dev and Debug mode\": \"\uac1c\ubc1c \ubc0f \ub514\ubc84\uadf8 \ubaa8\ub4dc\", \"Check for updates\": \"\uc5c5\ub370\uc774\ud2b8 \ud655\uc778\", \"Default\": \"\uae30\ubcf8\uac12\", \"Reset the settings to their default values.\": \"\uc124\uc815\uc744 \uae30\ubcf8\uac12\uc73c\ub85c \ucd08\uae30\ud654\ud569\ub2c8\ub2e4.\", \"Apply settings.\": \"\uc124\uc815 \uc801\uc6a9.\", \"You may need to restart the script for all changes to take effect.\": \"\ubaa8\ub4e0 \ubcc0\uacbd \uc0ac\ud56d\uc744 \uc801\uc6a9\ud558\ub824\uba74 \uc2a4\ud06c\ub9bd\ud2b8\ub97c \ub2e4\uc2dc \uc2dc\uc791\ud574\uc57c \ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Thank you for your donations!\": \"\uae30\ubd80\ud574 \uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4!\", \"Edit settings\": \"\uc124\uc815 \ud3b8\uc9d1\", \"Options\": \"\uc635\uc158\", \"Bug Report or Feature Request\": \"\ubc84\uadf8 \ubcf4\uace0 \ub610\ub294 \uae30\ub2a5 \uc694\uccad\", \"Bug report\nFeature request\n\nTell us what's wrong or request a new feature.\": \"\ubc84\uadf8 \ubcf4\uace0\n\uae30\ub2a5 \uc694\uccad\n\n\ubb38\uc81c\uc810\uc744 \uc54c\ub824\uc8fc\uac70\ub098 \uc0c8 \uae30\ub2a5\uc744 \uc694\uccad\ud558\uc138\uc694.\", \"Help\": \"\ub3c4\uc6c0\ub9d0\", \"Get help.\": \"\ub3c4\uc6c0\ub9d0\uc744 \ud655\uc778\ud569\ub2c8\ub2e4.\", \"Translate %1\": \"%1 \ubc88\uc5ed\", \"Help translating %1 to make it available to more people.\": \"%1\uc744 \ubc88\uc5ed\ud558\uc5ec \ub354 \ub9ce\uc740 \uc0ac\ub78c\ub4e4\uc774 \uc0ac\uc6a9\ud560 \uc218 \uc788\ub3c4\ub85d \ub3c4\uc640\uc8fc\uc138\uc694.\", \"New version\": \"\uc0c8 \ubc84\uc804\", \"This month, the %1 fund is $%2.\nThat's %3% of our monthly goal ( $%4 )\n\nClick on this button to join the development fund!\": \"\uc774\ub2ec %1 \uae30\uae08\uc740 $%2\uc785\ub2c8\ub2e4.\n\uc6d4\uac04 \ubaa9\ud45c($%4)\uc758 %3%\uc785\ub2c8\ub2e4.\n\n\uc774 \ubc84\ud2bc\uc744 \ud074\ub9ad\ud558\uc5ec \uac1c\ubc1c \uae30\uae08\uc5d0 \ucc38\uc5ec\ud558\uc138\uc694!\", \"Cancel\": \"\ucde8\uc18c\", \"Set a random value.\": \"\ub79c\ub364 \uac12\uc744 \uc124\uc815\ud569\ub2c8\ub2e4.\", \"Magic is happening\": \"\ub9c8\ubc95\uc774 \uc77c\uc5b4\ub098\uace0 \uc788\uc2b5\ub2c8\ub2e4\", \"Working\": \"\uc791\uc5c5 \uc911\", \"Open folder\": \"\ud3f4\ub354 \uc5f4\uae30\", \"Add item or category\": \"\ud56d\ubaa9 \ub610\ub294 \uce74\ud14c\uace0\ub9ac \ucd94\uac00\", \"Edit item or category\": \"\ud56d\ubaa9 \ub610\ub294 \uce74\ud14c\uace0\ub9ac \ud3b8\uc9d1\", \"Remove item or category\": \"\ud56d\ubaa9 \ub610\ub294 \uce74\ud14c\uace0\ub9ac \uc81c\uac70\", \"Item not found.\": \"\ud56d\ubaa9\uc744 \ucc3e\uc744 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Remove all\": \"\uc804\uccb4 \uc81c\uac70\", \"Start typing...\": \"\uc785\ub825 \uc2dc\uc791...\", \"Refresh\": \"\uc0c8\ub85c\uace0\uce68\", \"Category\": \"\uce74\ud14c\uace0\ub9ac\", \"Tip\": \"\ud301\", \"Select layers\": \"\ub808\uc774\uc5b4 \uc120\ud0dd\", \"Active composition\": \"\ud65c\uc131 \ucef4\ud3ec\uc9c0\uc158\", \"Selected compositions\": \"\uc120\ud0dd\ud55c \ucef4\ud3ec\uc9c0\uc158\", \"All compositions\": \"\ubaa8\ub4e0 \ucef4\ud3ec\uc9c0\uc158\", \"The '%1' panel is not installed.\": \"'%1' \ud328\ub110\uc774 \uc124\uce58\ub418\uc9c0 \uc54a\uc558\uc2b5\ub2c8\ub2e4.\", \"You're starting a process which can take some time.\": \"\uc2dc\uac04\uc774 \uac78\ub9b4 \uc218 \uc788\ub294 \ud504\ub85c\uc138\uc2a4\ub97c \uc2dc\uc791\ud569\ub2c8\ub2e4.\", \"Hiding layer controls will improve performance a lot. Do you want to toggle layer controls now?\": \"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864\uc744 \uc228\uae30\uba74 \uc131\ub2a5\uc774 \ud06c\uac8c \ud5a5\uc0c1\ub429\ub2c8\ub2e4. \uc9c0\uae08 \ub808\uc774\uc5b4 \ucee8\ud2b8\ub864\uc744 \uc804\ud658\ud558\uc2dc\uaca0\uc2b5\ub2c8\uae4c?\", \"If layer controls are already hidden, you can ignore this.\": \"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864\uc774 \uc774\ubbf8 \uc228\uaca8\uc838 \uc788\uc73c\uba74 \ubb34\uc2dc\ud558\uc154\ub3c4 \ub429\ub2c8\ub2e4.\", \"Permanently disable this alert.\": \"\uc774 \uc54c\ub9bc\uc744 \uc601\uad6c\uc801\uc73c\ub85c \ube44\ud65c\uc131\ud654\ud569\ub2c8\ub2e4.\", \"You can disable this alert dialog from showing before long operations.\nLayer Controls state won't be changed.\": \"\uc7a5\uc2dc\uac04 \uc791\uc5c5 \uc804\uc5d0 \uc774 \uc54c\ub9bc \ub300\ud654 \uc0c1\uc790\uac00 \ud45c\uc2dc\ub418\uc9c0 \uc54a\ub3c4\ub85d \ube44\ud65c\uc131\ud654\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\n\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864 \uc0c1\ud0dc\ub294 \ubcc0\uacbd\ub418\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\", \"This alert will be disabled.\": \"\uc774 \uc54c\ub9bc\uc774 \ube44\ud65c\uc131\ud654\ub429\ub2c8\ub2e4.\", \"Ignore\": \"\ubb34\uc2dc\", \"Hide layer controls\": \"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864 \uc228\uae30\uae30\", \"Magic is happening...\": \"\ub9c8\ubc95\uc774 \uc77c\uc5b4\ub098\uace0 \uc788\uc2b5\ub2c8\ub2e4...\", \"Project Root\": \"\ud504\ub85c\uc81d\ud2b8 \ub8e8\ud2b8\", \"Rectangle\": \"\uc9c1\uc0ac\uac01\ud615\", \"Rounded rectangle\": \"\ub465\uadfc \uc9c1\uc0ac\uac01\ud615\", \"The pseudo effect file does not exist.\": \"\uc218\ub3c4 \uc774\ud399\ud2b8 \ud30c\uc77c\uc774 \uc874\uc7ac\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\", \"Invalid pseudo effect file or match name.\": \"\uc720\ud6a8\ud558\uc9c0 \uc54a\uc740 \uc218\ub3c4 \uc774\ud399\ud2b8 \ud30c\uc77c \ub610\ub294 \ub9e4\uce58 \uc774\ub984\uc785\ub2c8\ub2e4.\", \"Sanity status: Unknown\": \"\uc0c1\ud0dc: \uc54c \uc218 \uc5c6\uc74c\", \"Sanity status: OK\": \"\uc0c1\ud0dc: \uc815\uc0c1\", \"Sanity status: Information\": \"\uc0c1\ud0dc: \uc815\ubcf4\", \"Sanity status: Warning\": \"\uc0c1\ud0dc: \uacbd\uace0\", \"Sanity status: Danger\": \"\uc0c1\ud0dc: \uc704\ud5d8\", \"Sanity status: Critical\": \"\uc0c1\ud0dc: \uc2ec\uac01\", \"Sanity status: Fatal\": \"\uc0c1\ud0dc: \uce58\uba85\uc801\", \"Unknown\": \"\uc54c \uc218 \uc5c6\uc74c\", \"Information\": \"\uc815\ubcf4\", \"Warning\": \"\uacbd\uace0\", \"Danger\": \"\uc704\ud5d8\", \"Critical\": \"\uc2ec\uac01\", \"Fatal\": \"\uce58\uba85\uc801\", \"Run all tests\": \"\ubaa8\ub4e0 \ud14c\uc2a4\ud2b8 \uc2e4\ud589\", \"Run all the tests and displays the results.\": \"\ubaa8\ub4e0 \ud14c\uc2a4\ud2b8\ub97c \uc2e4\ud589\ud558\uace0 \uacb0\uacfc\ub97c \ud45c\uc2dc\ud569\ub2c8\ub2e4.\", \"Toggle this test for the current project only.\": \"\ud604\uc7ac \ud504\ub85c\uc81d\ud2b8\uc5d0\uc11c\ub9cc \uc774 \ud14c\uc2a4\ud2b8\ub97c \uc804\ud658\ud569\ub2c8\ub2e4.\", \"Enable or disable automatic live-fix.\": \"\uc790\ub3d9 \uc2e4\uc2dc\uac04 \uc218\uc815\uc744 \ud65c\uc131\ud654 \ub610\ub294 \ube44\ud65c\uc131\ud654\ud569\ub2c8\ub2e4.\", \"Fix now.\": \"\uc9c0\uae08 \uc218\uc815.\", \"Run the current test.\": \"\ud604\uc7ac \ud14c\uc2a4\ud2b8\ub97c \uc2e4\ud589\ud569\ub2c8\ub2e4.\", \"Change the test settings.\": \"\ud14c\uc2a4\ud2b8 \uc124\uc815\uc744 \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Test every:\": \"\ud14c\uc2a4\ud2b8 \uc8fc\uae30:\", \"Size limit (MB)\": \"\ud06c\uae30 \uc81c\ud55c (MB)\", \"Maximum number of items\": \"\ucd5c\ub300 \ud56d\ubaa9 \uc218\", \"Maximum number of precompositions in the root folder\": \"\ub8e8\ud2b8 \ud3f4\ub354\uc758 \ucd5c\ub300 \ud504\ub9ac\ucef4\ud504 \uc218\", \"Default folder for precompositions\": \"\ud504\ub9ac\ucef4\ud504 \uae30\ubcf8 \ud3f4\ub354\", \"Maximum unused comps in the project\": \"\ud504\ub85c\uc81d\ud2b8 \ub0b4 \ucd5c\ub300 \ubbf8\uc0ac\uc6a9 \ucef4\ud504 \uc218\", \"Folder for main comps (leave empty for project root)\": \"\uba54\uc778 \ucef4\ud504 \ud3f4\ub354 (\ube44\uc6cc\ub450\uba74 \ud504\ub85c\uc81d\ud2b8 \ub8e8\ud2b8)\", \"Maximum memory (GB)\": \"\ucd5c\ub300 \uba54\ubaa8\ub9ac (GB)\", \"Maximum number of essential properties in a comp\": \"\ucef4\ud504 \ub0b4 \ucd5c\ub300 \ud544\uc218 \uc18d\uc131 \uc218\", \"Time limit before saving the project (mn)\": \"\ud504\ub85c\uc81d\ud2b8 \uc800\uc7a5 \uc804 \uc2dc\uac04 \uc81c\ud55c (\ubd84)\", \"Uptime limit (mn)\": \"\uac00\ub3d9 \uc2dc\uac04 \uc81c\ud55c (\ubd84)\", \"Composition Names\": \"\ucef4\ud3ec\uc9c0\uc158 \uc774\ub984\", \"Layer Names\": \"\ub808\uc774\uc5b4 \uc774\ub984\", \"Expression engine\": \"\ud45c\ud604\uc2dd \uc5d4\uc9c4\", \"Project size\": \"\ud504\ub85c\uc81d\ud2b8 \ud06c\uae30\", \"Project items\": \"\ud504\ub85c\uc81d\ud2b8 \ud56d\ubaa9\", \"Duplicated footages\": \"\uc911\ubcf5 \ud478\ud2f0\uc9c0\", \"Unused footages\": \"\ubbf8\uc0ac\uc6a9 \ud478\ud2f0\uc9c0\", \"Precompositions\": \"\ud504\ub9ac\ucef4\ud3ec\uc9c0\uc158\", \"Main compositions\": \"\uba54\uc778 \ucef4\ud3ec\uc9c0\uc158\", \"Memory in use\": \"\uc0ac\uc6a9 \uc911\uc778 \uba54\ubaa8\ub9ac\", \"Essential properties\": \"\ud544\uc218 \uc18d\uc131\", \"Time since last save\": \"\ub9c8\uc9c0\ub9c9 \uc800\uc7a5 \uc774\ud6c4 \uc2dc\uac04\", \"Up time\": \"\uac00\ub3d9 \uc2dc\uac04\", \"The project needs to be saved first.\": \"\uba3c\uc800 \ud504\ub85c\uc81d\ud2b8\ub97c \uc800\uc7a5\ud574\uc57c \ud569\ub2c8\ub2e4.\", \"Project\": \"\ud504\ub85c\uc81d\ud2b8\", \"Set a note for the current project\": \"\ud604\uc7ac \ud504\ub85c\uc81d\ud2b8\uc5d0 \uba54\ubaa8 \uc124\uc815\", \"Composition\": \"\ucef4\ud3ec\uc9c0\uc158\", \"Set a note for the current composition\": \"\ud604\uc7ac \ucef4\ud3ec\uc9c0\uc158\uc5d0 \uba54\ubaa8 \uc124\uc815\", \"Text file\": \"\ud14d\uc2a4\ud2b8 \ud30c\uc77c\", \"Select the file where to save the notes.\": \"\uba54\ubaa8\ub97c \uc800\uc7a5\ud560 \ud30c\uc77c\uc744 \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Reloads the note\": \"\uba54\ubaa8 \ub2e4\uc2dc \ubd88\ub7ec\uc624\uae30\", \"New line: Ctrl/Cmd + Enter\": \"\uc0c8 \uc904: Ctrl/Cmd + Enter\", \"Show envelops\": \"\uc5d4\ubca8\ub85c\ud504 \ud45c\uc2dc\", \"Show noodles\": \"\ub204\ub4e4 \ud45c\uc2dc\", \"Create new composition\": \"\uc0c8 \ucef4\ud3ec\uc9c0\uc158 \ub9cc\ub4e4\uae30\", \"[Alt]: Create and build in a new composition.\": \"[Alt]: \uc0c8 \ucef4\ud3ec\uc9c0\uc158\uc5d0\uc11c \ub9cc\ub4e4\uace0 \ube4c\ub4dc\ud569\ub2c8\ub2e4.\", \"Create OCO Meta-rig\": \"OCO \uba54\ud0c0\ub9ac\uadf8 \ub9cc\ub4e4\uae30\", \"Meta-Rig name\": \"\uba54\ud0c0\ub9ac\uadf8 \uc774\ub984\", \"Meta-Rig settings.\": \"\uba54\ud0c0\ub9ac\uadf8 \uc124\uc815.\", \"Meta-Rig\": \"\uba54\ud0c0\ub9ac\uadf8\", \"Build selected Meta-Rig.\": \"\uc120\ud0dd\ud55c \uba54\ud0c0\ub9ac\uadf8\ub97c \ube4c\ub4dc\ud569\ub2c8\ub2e4.\", \"Create a new Meta-Rig from selected bones or create a new category.\": \"\uc120\ud0dd\ud55c \ubf08\ub300\uc5d0\uc11c \uc0c8 \uba54\ud0c0\ub9ac\uadf8\ub97c \ub9cc\ub4e4\uac70\ub098 \uc0c8 \uce74\ud14c\uace0\ub9ac\ub97c \ub9cc\ub4ed\ub2c8\ub2e4.\", \"Edit the selected Meta-Rig or category.\": \"\uc120\ud0dd\ud55c \uba54\ud0c0\ub9ac\uadf8 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\", \"Remove the selected Meta-Rig or category from library.\": \"\ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0\uc11c \uc120\ud0dd\ud55c \uba54\ud0c0\ub9ac\uadf8 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \uc81c\uac70\ud569\ub2c8\ub2e4.\", \"Sorry, the OCO library folder can't be found.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, OCO \ub77c\uc774\ube0c\ub7ec\ub9ac \ud3f4\ub354\ub97c \ucc3e\uc744 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Select the OCO.config file.\": \"OCO.config \ud30c\uc77c\uc744 \uc120\ud0dd\ud558\uc138\uc694.\", \"Create OCO Meta-Rig\": \"OCO \uba54\ud0c0\ub9ac\uadf8 \ub9cc\ub4e4\uae30\", \"Selected bones\": \"\uc120\ud0dd\ud55c \ubf08\ub300\", \"All bones\": \"\ubaa8\ub4e0 \ubf08\ub300\", \"Icon\": \"\uc544\uc774\ucf58\", \"Choose an icon to asssicate with the new Meta-Rig\": \"\uc0c8 \uba54\ud0c0\ub9ac\uadf8\uc5d0 \uc5f0\uacb0\ud560 \uc544\uc774\ucf58\uc744 \uc120\ud0dd\ud558\uc138\uc694\", \"Meta-Rig Name\": \"\uba54\ud0c0\ub9ac\uadf8 \uc774\ub984\", \"Choose the name of the meta-rig.\": \"\uba54\ud0c0\ub9ac\uadf8\uc758 \uc774\ub984\uc744 \uc120\ud0dd\ud558\uc138\uc694.\", \"Character height:\": \"\uce90\ub9ad\ud130 \ub192\uc774:\", \"Export the selected armature to an OCO Meta-Rig file.\": \"\uc120\ud0dd\ud55c \uc544\ub9c8\ucd94\uc5b4\ub97c OCO \uba54\ud0c0\ub9ac\uadf8 \ud30c\uc77c\ub85c \ub0b4\ubcf4\ub0c5\ub2c8\ub2e4.\", \"Please, choose a name for the meta-rig\": \"\uba54\ud0c0\ub9ac\uadf8\uc758 \uc774\ub984\uc744 \uc120\ud0dd\ud574\uc8fc\uc138\uc694\", \"Sorry, we can't save an OCO file directly in the current category.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \ud604\uc7ac \uce74\ud14c\uace0\ub9ac\uc5d0 \uc9c1\uc811 OCO \ud30c\uc77c\uc744 \uc800\uc7a5\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Run script\": \"\uc2a4\ud06c\ub9bd\ud2b8 \uc2e4\ud589\", \"All categories\": \"\ubaa8\ub4e0 \uce74\ud14c\uace0\ub9ac\", \"Dockable Scripts\": \"\ub3c4\ud0b9 \uac00\ub2a5 \uc2a4\ud06c\ub9bd\ud2b8\", \"Ae Scripts\": \"AE \uc2a4\ud06c\ub9bd\ud2b8\", \"Startup Scripts\": \"\uc2dc\uc791 \uc2a4\ud06c\ub9bd\ud2b8\", \"Shutdown Scripts\": \"\uc885\ub8cc \uc2a4\ud06c\ub9bd\ud2b8\", \"Script settings\": \"\uc2a4\ud06c\ub9bd\ud2b8 \uc124\uc815\", \"Select icon\": \"\uc544\uc774\ucf58 \uc120\ud0dd\", \"Script name\": \"\uc2a4\ud06c\ub9bd\ud2b8 \uc774\ub984\", \"Open scripts with...\": \"\uc2a4\ud06c\ub9bd\ud2b8 \uc5f4\uae30...\", \"Select an application to open the scripts.\nLeave the field empty to use the system default.\": \"\uc2a4\ud06c\ub9bd\ud2b8\ub97c \uc5f4 \uc560\ud50c\ub9ac\ucf00\uc774\uc158\uc744 \uc120\ud0dd\ud558\uc138\uc694.\n\uc2dc\uc2a4\ud15c \uae30\ubcf8\uac12\uc744 \uc0ac\uc6a9\ud558\ub824\uba74 \ube44\uc6cc\ub450\uc138\uc694.\", \"Use the Duik quick editor.\": \"Duik \ube60\ub978 \ud3b8\uc9d1\uae30\ub97c \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Use the Duik quick script editor to edit the selected script.\": \"Duik \ube60\ub978 \uc2a4\ud06c\ub9bd\ud2b8 \ud3b8\uc9d1\uae30\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc120\ud0dd\ud55c \uc2a4\ud06c\ub9bd\ud2b8\ub97c \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\", \"Run the selected script.\n\n[Alt]: Run the script as a standard script even if it's a dockable ScriptUI panel.\": \"\uc120\ud0dd\ud55c \uc2a4\ud06c\ub9bd\ud2b8\ub97c \uc2e4\ud589\ud569\ub2c8\ub2e4.\n\n[Alt]: \ub3c4\ud0b9 \uac00\ub2a5\ud55c ScriptUI \ud328\ub110\uc774\ub354\ub77c\ub3c4 \ud45c\uc900 \uc2a4\ud06c\ub9bd\ud2b8\ub85c \uc2e4\ud589\ud569\ub2c8\ub2e4.\", \"Add a new script or a category to the library.\": \"\ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0 \uc0c8 \uc2a4\ud06c\ub9bd\ud2b8 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Removes the selected script or category from the library.\": \"\ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0\uc11c \uc120\ud0dd\ud55c \uc2a4\ud06c\ub9bd\ud2b8 \ub610\ub294 \uce74\ud14c\uace0\ub9ac\ub97c \uc81c\uac70\ud569\ub2c8\ub2e4.\", \"Edit the selected script.\n\n[Alt]: Use the Duik quick editor.\": \"\uc120\ud0dd\ud55c \uc2a4\ud06c\ub9bd\ud2b8\ub97c \ud3b8\uc9d1\ud569\ub2c8\ub2e4.\n\n[Alt]: Duik \ube60\ub978 \ud3b8\uc9d1\uae30\ub97c \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Script not found\": \"\uc2a4\ud06c\ub9bd\ud2b8\ub97c \ucc3e\uc744 \uc218 \uc5c6\uc74c\", \"Sorry, we can't save a script directly in the current category.\": \"\uc8c4\uc1a1\ud569\ub2c8\ub2e4, \ud604\uc7ac \uce74\ud14c\uace0\ub9ac\uc5d0 \uc9c1\uc811 \uc2a4\ud06c\ub9bd\ud2b8\ub97c \uc800\uc7a5\ud560 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4.\", \"Add new scripts to the library.\": \"\ub77c\uc774\ube0c\ub7ec\ub9ac\uc5d0 \uc0c8 \uc2a4\ud06c\ub9bd\ud2b8\ub97c \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"Home panel enabled\": \"\ud648 \ud328\ub110 \ud65c\uc131\ud654\ub428\", \"The home panel helps Duik to launch much faster.\nDisabling it may make the launch time of Duik much slower.\": \"\ud648 \ud328\ub110\uc740 Duik\uc744 \ud6e8\uc52c \ube60\ub974\uac8c \uc2dc\uc791\ud558\ub3c4\ub85d \ub3c4\uc640\uc90d\ub2c8\ub2e4.\n\ube44\ud65c\uc131\ud654\ud558\uba74 Duik \uc2dc\uc791 \uc2dc\uac04\uc774 \ud6e8\uc52c \ub290\ub824\uc9c8 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Home panel disabled\": \"\ud648 \ud328\ub110 \ube44\ud65c\uc131\ud654\ub428\", \"Layer controls alert enabled\": \"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864 \uc54c\ub9bc \ud65c\uc131\ud654\ub428\", \"You can disable the \\\"Layer controls\\\" alert dialog which is shown before long operations.\": \"\uc7a5\uc2dc\uac04 \uc791\uc5c5 \uc804\uc5d0 \ud45c\uc2dc\ub418\ub294 \\\"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864\\\" \uc54c\ub9bc \ub300\ud654 \uc0c1\uc790\ub97c \ube44\ud65c\uc131\ud654\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\", \"Layer controls alert disabled\": \"\ub808\uc774\uc5b4 \ucee8\ud2b8\ub864 \uc54c\ub9bc \ube44\ud65c\uc131\ud654\ub428\", \"Composition tools (crop, change settings...)\": \"\ucef4\ud3ec\uc9c0\uc158 \ub3c4\uad6c (\uc790\ub974\uae30, \uc124\uc815 \ubcc0\uacbd...)\", \"Layer\": \"\ub808\uc774\uc5b4\", \"Text\": \"\ud14d\uc2a4\ud2b8\", \"Text tools (rename, search and replace...)\": \"\ud14d\uc2a4\ud2b8 \ub3c4\uad6c (\uc774\ub984 \ubcc0\uacbd, \ucc3e\uae30 \ubc0f \ubc14\uafb8\uae30...)\", \"Scripting\": \"\uc2a4\ud06c\ub9bd\ud305\", \"Scripting tools\": \"\uc2a4\ud06c\ub9bd\ud305 \ub3c4\uad6c\", \"Crops the selected precompositions using the bounds of their masks.\": \"\ub9c8\uc2a4\ud06c \ubc94\uc704\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc120\ud0dd\ud55c \ud504\ub9ac\ucef4\ud504\ub97c \uc790\ub985\ub2c8\ub2e4.\", \"Sets the current or selected composition(s) settings, also changing the settings of all precompositions.\": \"\ud604\uc7ac \ub610\ub294 \uc120\ud0dd\ud55c \ucef4\ud3ec\uc9c0\uc158\uc758 \uc124\uc815\uc744 \ubcc0\uacbd\ud558\uba70, \ubaa8\ub4e0 \ud504\ub9ac\ucef4\ud504 \uc124\uc815\ub3c4 \ubcc0\uacbd\ud569\ub2c8\ub2e4.\", \"Rename\": \"\uc774\ub984 \ubcc0\uacbd\", \"Renames the selected items (layers, pins, project items...)\": \"\uc120\ud0dd\ud55c \ud56d\ubaa9\uc758 \uc774\ub984\uc744 \ubcc0\uacbd\ud569\ub2c8\ub2e4 (\ub808\uc774\uc5b4, \ud540, \ud504\ub85c\uc81d\ud2b8 \ud56d\ubaa9...)\", \"Puppet pins\": \"\ud37c\ud3ab \ud540\", \"Update expressions\": \"\ud45c\ud604\uc2dd \uc5c5\ub370\uc774\ud2b8\", \"Automatically updates the expressions.\": \"\ud45c\ud604\uc2dd\uc744 \uc790\ub3d9\uc73c\ub85c \uc5c5\ub370\uc774\ud2b8\ud569\ub2c8\ub2e4.\", \"Remove the first digits\": \"\uc55e\uc790\ub9ac \uc22b\uc790 \uc81c\uac70\", \"digits\": \"\uc790\ub9bf\uc218\", \"Remove the last digits\": \"\ub4b7\uc790\ub9ac \uc22b\uc790 \uc81c\uac70\", \"Number from\": \"\ubc88\ud638 \uc2dc\uc791\", \"Reverse\": \"\ubc18\uc804\", \"Number from last to first.\": \"\ub9c8\uc9c0\ub9c9\uc5d0\uc11c \uccab \ubc88\uc9f8\ub85c \ubc88\ud638 \ub9e4\uae30\uae30.\", \"Prefix\": \"\uc811\ub450\uc0ac\", \"Suffix\": \"\uc811\ubbf8\uc0ac\", \"Search and replace\": \"\ucc3e\uae30 \ubc0f \ubc14\uafb8\uae30\", \"Searches and replaces text in the project.\": \"\ud504\ub85c\uc81d\ud2b8\uc5d0\uc11c \ud14d\uc2a4\ud2b8\ub97c \ucc3e\uace0 \ubc14\uafc9\ub2c8\ub2e4.\", \"Expressions\": \"\ud45c\ud604\uc2dd\", \"Texts\": \"\ud14d\uc2a4\ud2b8\", \"All Compositions\": \"\ubaa8\ub4e0 \ucef4\ud3ec\uc9c0\uc158\", \"Compositions\": \"\ucef4\ud3ec\uc9c0\uc158\", \"Footages\": \"\ud478\ud2f0\uc9c0\", \"Folders\": \"\ud3f4\ub354\", \"All items\": \"\ubaa8\ub4e0 \ud56d\ubaa9\", \"Selected items\": \"\uc120\ud0dd\ud55c \ud56d\ubaa9\", \"Case sensitive\": \"\ub300\uc18c\ubb38\uc790 \uad6c\ubd84\", \"Search\": \"\ucc3e\uae30\", \"Replace\": \"\ubc14\uafb8\uae30\", \"Search and replace text in the project.\": \"\ud504\ub85c\uc81d\ud2b8\uc5d0\uc11c \ud14d\uc2a4\ud2b8\ub97c \ucc3e\uace0 \ubc14\uafc9\ub2c8\ub2e4.\", \"Script library\": \"\uc2a4\ud06c\ub9bd\ud2b8 \ub77c\uc774\ube0c\ub7ec\ub9ac\", \"Quickly access and run all your scripts and panels.\": \"\ubaa8\ub4e0 \uc2a4\ud06c\ub9bd\ud2b8\uc640 \ud328\ub110\uc5d0 \ube60\ub974\uac8c \uc811\uadfc\ud558\uace0 \uc2e4\ud589\ud569\ub2c8\ub2e4.\", \"Scriptify expression\": \"\ud45c\ud604\uc2dd \uc2a4\ud06c\ub9bd\ud2b8\ud654\", \"Generate a handy ExtendScript code to easily include the selected expression into a script.\": \"\uc120\ud0dd\ud55c \ud45c\ud604\uc2dd\uc744 \uc2a4\ud06c\ub9bd\ud2b8\uc5d0 \uc27d\uac8c \ud3ec\ud568\uc2dc\ud0ac \uc218 \uc788\ub294 ExtendScript \ucf54\ub4dc\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Script editor\": \"\uc2a4\ud06c\ub9bd\ud2b8 \ud3b8\uc9d1\uae30\", \"A quick editor for editing and running simple scripts and snippets.\": \"\uac04\ub2e8\ud55c \uc2a4\ud06c\ub9bd\ud2b8\uc640 \ucf54\ub4dc \uc870\uac01\uc744 \ud3b8\uc9d1\ud558\uace0 \uc2e4\ud589\ud558\ub294 \ube60\ub978 \ud3b8\uc9d1\uae30\uc785\ub2c8\ub2e4.\", \"Sanity status\": \"\uc0c1\ud0dc \uac80\uc0ac\", \"[Alt]: One controller for all layers.\n[Ctrl]: Parent layers to the controllers.\": \"[Alt]: \ubaa8\ub4e0 \ub808\uc774\uc5b4\uc5d0 \ud558\ub098\uc758 \ucee8\ud2b8\ub864\ub7ec.\n[Ctrl]: \ub808\uc774\uc5b4\ub97c \ucee8\ud2b8\ub864\ub7ec\uc5d0 \ubd80\ubaa8 \uc5f0\uacb0.\", \"Art\": \"\uc544\ud2b8\", \"Adjustment\": \"\uc870\uc815\", \"Reposition the anchor points of all selected layers.\": \"\uc120\ud0dd\ud55c \ubaa8\ub4e0 \ub808\uc774\uc5b4\uc758 \uc575\ucee4 \ud3ec\uc778\ud2b8\ub97c \uc7ac\ubc30\uce58\ud569\ub2c8\ub2e4.\", \"Use Full bones (with envelop and noodle)\": \"\uc804\uccb4 \ubf08\ub300 \uc0ac\uc6a9 (\uc5d4\ubca8\ub85c\ud504\uc640 \ub204\ub4e4 \ud3ec\ud568)\", \"Use Light bones\": \"\uac00\ubcbc\uc6b4 \ubf08\ub300 \uc0ac\uc6a9\", \"Include masks\": \"\ub9c8\uc2a4\ud06c \ud3ec\ud568\", \"Use the masks too to compute the bounds of the layers when repositionning the anchor point.\": \"\uc575\ucee4 \ud3ec\uc778\ud2b8\ub97c \uc7ac\ubc30\uce58\ud560 \ub54c \ub808\uc774\uc5b4 \ubc94\uc704\ub97c \uacc4\uc0b0\ud558\uae30 \uc704\ud574 \ub9c8\uc2a4\ud06c\ub3c4 \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\", \"Margin\": \"\uc5ec\ubc31\", \"Select the layer (texture/map)\": \"\ub808\uc774\uc5b4 \uc120\ud0dd (\ud14d\uc2a4\ucc98/\ub9f5)\", \"Select the layer (texture) to use as an effector.\": \"\uc774\ud399\ud130\ub85c \uc0ac\uc6a9\ud560 \ub808\uc774\uc5b4(\ud14d\uc2a4\ucc98)\ub97c \uc120\ud0dd\ud569\ub2c8\ub2e4.\", \"Connect properties\": \"\uc18d\uc131 \uc5f0\uacb0\", \"Align layers.\": \"\ub808\uc774\uc5b4\ub97c \uc815\ub82c\ud569\ub2c8\ub2e4.\", \"All selected layers will be aligned\nto the last selected one.\": \"\uc120\ud0dd\ud55c \ubaa8\ub4e0 \ub808\uc774\uc5b4\uac00\n\ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc120\ud0dd\ud55c \ub808\uc774\uc5b4\uc5d0 \uc815\ub82c\ub429\ub2c8\ub2e4.\", \"Clean Keyframes.\nAutomates the animation process, and makes it easier to control:\n- Anticipation\n- Motion interpolation\n- Overlap\n- Follow through or Bounce\n- Soft Body simulation\n\": \"\ud0a4\ud504\ub808\uc784 \uc815\ub9ac.\n\uc560\ub2c8\uba54\uc774\uc158 \ud504\ub85c\uc138\uc2a4\ub97c \uc790\ub3d9\ud654\ud558\uace0 \ub2e4\uc74c\uc744 \uc27d\uac8c \uc81c\uc5b4\ud569\ub2c8\ub2e4:\n- \uc608\ube44 \ub3d9\uc791\n- \ubaa8\uc158 \ubcf4\uac04\n- \uc624\ubc84\ub7a9\n- \ud314\ub85c\uc2a4\ub8e8 \ub610\ub294 \ubc14\uc6b4\uc2a4\n- \uc18c\ud504\ud2b8 \ubc14\ub514 \uc2dc\ubbac\ub808\uc774\uc158\n\", \"Alive (anticipation + interpolation + follow through)\": \"\uc0b4\uc544\uc788\ub294 (\uc608\ube44 \ub3d9\uc791 + \ubcf4\uac04 + \ud314\ub85c\uc2a4\ub8e8)\", \"The default animation, with anticipation, motion interpolation and follow through.\": \"\uc608\ube44 \ub3d9\uc791, \ubaa8\uc158 \ubcf4\uac04, \ud314\ub85c\uc2a4\ub8e8\uac00 \ud3ec\ud568\ub41c \uae30\ubcf8 \uc560\ub2c8\uba54\uc774\uc158.\", \"Inanimate (interpolation + follow through)\": \"\ubb34\uc0dd\ubb3c (\ubcf4\uac04 + \ud314\ub85c\uc2a4\ub8e8)\", \"For inanimate objects.\nDeactivate the anticipation.\": \"\ubb34\uc0dd\ubb3c \uac1d\uccb4\uc6a9.\n\uc608\ube44 \ub3d9\uc791\uc744 \ube44\ud65c\uc131\ud654\ud569\ub2c8\ub2e4.\", \"True stop (anticipation + interpolation)\": \"\uc644\uc804 \uc815\uc9c0 (\uc608\ube44 \ub3d9\uc791 + \ubcf4\uac04)\", \"Deactivate the follow through animation.\": \"\ud314\ub85c\uc2a4\ub8e8 \uc560\ub2c8\uba54\uc774\uc158\uc744 \ube44\ud65c\uc131\ud654\ud569\ub2c8\ub2e4.\", \"Exact keyframes (interpolation only)\": \"\uc815\ud655\ud55c \ud0a4\ud504\ub808\uc784 (\ubcf4\uac04\ub9cc)\", \"Deactivates anticipation and follow through, and use the exact keyframe values.\nGenerate only the motion interpolation.\": \"\uc608\ube44 \ub3d9\uc791\uacfc \ud314\ub85c\uc2a4\ub8e8\ub97c \ube44\ud65c\uc131\ud654\ud558\uace0 \uc815\ud655\ud55c \ud0a4\ud504\ub808\uc784 \uac12\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4.\n\ubaa8\uc158 \ubcf4\uac04\ub9cc \uc0dd\uc131\ud569\ub2c8\ub2e4.\", \"Spring (follow through only)\": \"\uc2a4\ud504\ub9c1 (\ud314\ub85c\uc2a4\ub8e8\ub9cc)\", \"Use AE keyframe interpolation and just animate the follow through.\": \"AE \ud0a4\ud504\ub808\uc784 \ubcf4\uac04\uc744 \uc0ac\uc6a9\ud558\uace0 \ud314\ub85c\uc2a4\ub8e8\ub9cc \uc560\ub2c8\uba54\uc774\uc158\ud569\ub2c8\ub2e4.\", \"Spring (no simulation)\": \"\uc2a4\ud504\ub9c1 (\uc2dc\ubbac\ub808\uc774\uc158 \uc5c6\uc74c)\", \"A lighter version of the spring, which works only if the property has it's own keyframes.\nMotion from parent layers or the anchor point of the layer itself will be ignored.\": \"\uc2a4\ud504\ub9c1\uc758 \uac00\ubcbc\uc6b4 \ubc84\uc804\uc73c\ub85c, \uc18d\uc131\uc5d0 \uc790\uccb4 \ud0a4\ud504\ub808\uc784\uc774 \uc788\ub294 \uacbd\uc6b0\uc5d0\ub9cc \uc791\ub3d9\ud569\ub2c8\ub2e4.\n\ubd80\ubaa8 \ub808\uc774\uc5b4\ub098 \ub808\uc774\uc5b4 \uc790\uccb4\uc758 \uc575\ucee4 \ud3ec\uc778\ud2b8\uc5d0\uc11c\uc758 \ubaa8\uc158\uc740 \ubb34\uc2dc\ub429\ub2c8\ub2e4.\", \"Bounce (follow through only)\": \"\ubc14\uc6b4\uc2a4 (\ud314\ub85c\uc2a4\ub8e8\ub9cc)\", \"Use AE keyframe interpolation and just animate a bounce at the end.\": \"AE \ud0a4\ud504\ub808\uc784 \ubcf4\uac04\uc744 \uc0ac\uc6a9\ud558\uace0 \ub05d\uc5d0\uc11c \ubc14\uc6b4\uc2a4\ub9cc \uc560\ub2c8\uba54\uc774\uc158\ud569\ub2c8\ub2e4.\", \"Bounce (no simulation)\": \"\ubc14\uc6b4\uc2a4 (\uc2dc\ubbac\ub808\uc774\uc158 \uc5c6\uc74c)\", \"Limits\": \"\uc81c\ud55c\", \"Limit the value the property can get.\": \"\uc18d\uc131\uc774 \uac00\uc9c8 \uc218 \uc788\ub294 \uac12\uc744 \uc81c\ud55c\ud569\ub2c8\ub2e4.\", \"Adjusts the exposure of the animation\n(changes and animates the framerate)\n\n[Ctrl]: (Try to) auto-compute the best values.\": \"\uc560\ub2c8\uba54\uc774\uc158\uc758 \ub178\ucd9c\uc744 \uc870\uc815\ud569\ub2c8\ub2e4\n(\ud504\ub808\uc784 \ub808\uc774\ud2b8\ub97c \ubcc0\uacbd\ud558\uace0 \uc560\ub2c8\uba54\uc774\uc158\ud569\ub2c8\ub2e4)\n\n[Ctrl]: \ucd5c\uc801\uc758 \uac12\uc744 \uc790\ub3d9 \uacc4\uc0b0\ud569\ub2c8\ub2e4.\", \"Automatically rig armatures (use the Links & constraints tab for more options).\": \"\uc544\ub9c8\ucd94\uc5b4\ub97c \uc790\ub3d9\uc73c\ub85c \ub9ac\uae45\ud569\ub2c8\ub2e4 (\ucd94\uac00 \uc635\uc158\uc740 \ub9c1\ud06c & \ucee8\uc2a4\ud2b8\ub808\uc778\ud2b8 \ud0ed \uc0ac\uc6a9).\", \"3-Layer rig:\": \"3\ub808\uc774\uc5b4 \ub9ac\uadf8:\", \"Long chain rig:\": \"\uae34 \uccb4\uc778 \ub9ac\uadf8:\", \"Create a root controller\": \"\ub8e8\ud2b8 \ucee8\ud2b8\ub864\ub7ec \uc0dd\uc131\", \"Baking:\": \"\ubca0\uc774\ud06c \uc911:\", \"Bake envelops\": \"\uc5d4\ubca8\ub85c\ud504 \ubca0\uc774\ud06c\", \"Remove deactivated noodles.\": \"\ube44\ud65c\uc131\ud654\ub41c \ub204\ub4e4 \uc81c\uac70.\", \"Add a list to the selected properties.\": \"\uc120\ud0dd\ud55c \uc18d\uc131\uc5d0 \ubaa9\ub85d\uc744 \ucd94\uac00\ud569\ub2c8\ub2e4.\", \"[Alt]: Adds a keyframe to the second slot (and quickly reveal it with [U] in the timeline).\": \"[Alt]: \ub450 \ubc88\uc9f8 \uc2ac\ub86f\uc5d0 \ud0a4\ud504\ub808\uc784\uc744 \ucd94\uac00\ud569\ub2c8\ub2e4 (\ud0c0\uc784\ub77c\uc778\uc5d0\uc11c [U]\ub85c \ube60\ub974\uac8c \ud45c\uc2dc).\"}", "Duik_ko_KR.json", "tr");
