 * @return {Boolean}
 */
DuAELayer.hasExpressions = function (layer) {
    var p = new DuAEProperty(layer);
    return p.hasExpressions();
}

/**
 * Checks if the layer has some masks
 * @param {Layer} layer The layer
 * @return {Boolean}
