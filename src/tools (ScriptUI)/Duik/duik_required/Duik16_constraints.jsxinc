    {
        var comp = DuAEF.DuAE.Project.getActiveComp();
        if (!comp) return;

        //create controller
        var layer = shared.createController( DuAEF.Duik.Controller.Types.SLIDER )[0];

        //ready
        connectorPick();
    }

    function ui_connectorDoubleSliderButton_clicked()
    {
        var comp = DuAEF.DuAE.Project.getActiveComp();
        if (!comp) return;

        //create controller
        var layer = shared.createController( DuAEF.Duik.Controller.Types.DOUBLE_SLIDER )[0];

        //ready
        connectorPick();
    }

    function ui_connectorAngleButton_clicked()
    {

        var comp = DuAEF.DuAE.Project.getActiveComp();
        if (!comp) return;

        //create controller
        var layer = shared.createController( DuAEF.Duik.Controller.Types.ANGLE )[0];

