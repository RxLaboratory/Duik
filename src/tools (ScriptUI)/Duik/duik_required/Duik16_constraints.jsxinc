    function ui_locatorButton_clicked ()
    {
        var layers = DuAEF.DuAE.Comp.getSelectedLayers();

        app.beginUndoGroup( 'Duik | ' + "Add Locator(s)" );

        if ( layers.length )
        {
            new Iterator( layers ).do( function( layer )
