/**
	* Constructs a Controller
	* @class Controller
	* @classdesc A Controller.
	* @param {ShapeLayer} layer - The layer of the controller
	* @property {ShapeLayer} layer - The Shape layer of the controller
	* @property {DuAEF.Duik.Controller.Types} type - The type of the controller
	* @property {PropertyGroup} effect - The effect on the layer
*/
function Controller(layer)
{
	this.layer = layer;
	var effect = this.layer.effect(DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_SLIDER);
	if (!effect) effect = this.layer.effect(DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_DOUBLE_SLIDER);
	if (!effect) effect = this.layer.effect(DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_ANGLE);
	if (!effect) effect = this.layer.effect(DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER);
	if (effect)
	{
		this.effect = effect;
		this.effectInfo = new PropertyInfo(effect);
	}
	else this.effect = null;
}

/**
	* Sets a new display size for the controller
	* @memberof Controller
	* @param {int} [size] - The controller size, a percentage. By default, it will adjust automatically depending on comp size and DuAEF.Duik.Controller.sizeHint
*/
Controller.prototype.setSize = function (size)
{
	if (this.layer == null) return;
	if (this.effect == null) return;

	this.effect = this.effectInfo.getProperty();

	if (size == undefined)
	{
		var comp = this.layer.containingComp;
		size = comp.width/2 + comp.height/2;
		if (DuAEF.Duik.Controller.sizeHint == DuAEF.Duik.Sizes.SMALL) size = Math.floor(size*100/1500);
		else if (DuAEF.Duik.Controller.sizeHint == DuAEF.Duik.Sizes.MEDIUM) size = Math.floor(size*150/1500);
		else if (DuAEF.Duik.Controller.sizeHint == DuAEF.Duik.Sizes.LARGE) size = Math.floor(size*200/1500);
		else if (DuAEF.Duik.Controller.sizeHint == DuAEF.Duik.Sizes.CUSTOM) size = DuAEF.Duik.Controller.size;
	}
	var index1 = 4;
	var index2 = 10;

	if (this.effect.matchName == DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_SLIDER || this.effect.matchName == DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_DOUBLE_SLIDER || this.effect.matchName == DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER_ANGLE)
	{
		index1 = 3;
		index2 = 8;
	}

	this.effect(index1).setValue(size);
	this.effect(index2).setValue(size);
}

/**
	* Sets a new display color for the controller
	* @memberof Controller
	* @param {float[]} color - The new color
*/
Controller.prototype.setColor = function (color)
{
	if (this.effect == null) return;
	if (color == undefined) color = DuAEF.Duik.Controller.color;
	if (color == DuAEF.DuJS.Color.Colors.RANDOM) color = DuAEF.DuJS.Color.random();

	this.effect = this.effectInfo.getProperty();

	this.effect(2).setValue(color);
}

/**
 * "Bakes" the appearance of the Controller,<br />
 * in order to improve performance and simplify the use of the rig when animating.
 * @memberof Controller
 */
Controller.prototype.bakeAppearance = function ()
{
	if (this.layer instanceof ShapeLayer)
	{
		var iconGroup = this.layer('ADBE Root Vectors Group')("Icon");

		if (iconGroup)
		{
			DuAEF.DuAE.Property.removeExpression(iconGroup.transform.scale);
			DuAEF.DuAE.Property.removeExpression(iconGroup.transform.position);
			DuAEF.DuAE.Property.removeExpression(iconGroup.transform.rotation);
			DuAEF.DuAE.Property.removeExpression(iconGroup.transform.opacity);

			var iconContent = iconGroup.property("ADBE Vectors Group");

			var stroke = iconContent.property("ADBE Vector Graphic - Stroke");
			if (stroke)
			{
				DuAEF.DuAE.Property.removeExpression( stroke.property("ADBE Vector Stroke Color") );
			}

			var fill = iconContent.property("ADBE Vector Graphic - Fill");
			if (fill)
			{
				DuAEF.DuAE.Property.removeExpression( fill.property("ADBE Vector Fill Color") );
			}

			var shape1 = iconContent.property("ADBE Vector Shape - Ellipse");
			if (shape1)
			{
				DuAEF.DuAE.Property.removeExpression( shape1.property('ADBE Vector Ellipse Size') );
			}
		}

		var anchorGroup = this.layer('ADBE Root Vectors Group').property("Anchor");

		if (anchorGroup)
		{
			DuAEF.DuAE.Property.removeExpression(anchorGroup.transform.scale);

			var anchorContent = anchorGroup.property("ADBE Vectors Group");

			var stroke = anchorContent.property("ADBE Vector Graphic - Stroke");
			if (stroke)
			{
				DuAEF.DuAE.Property.removeExpression( stroke.property("ADBE Vector Stroke Color") );
				DuAEF.DuAE.Property.removeExpression( stroke.property("ADBE Vector Stroke Width") );
			}

			var rectangle = anchorContent.property("ADBE Vector Shape - Rect");
			if (rectangle)
			{
				DuAEF.DuAE.Property.removeExpression( rectangle.property("ADBE Vector Rect Size") );
				DuAEF.DuAE.Property.removeExpression( rectangle.property("ADBE Vector Rect Roundness") );
			}

			var circle = anchorContent.property("ADBE Vector Shape - Ellipse");
			if (circle)
			{
				DuAEF.DuAE.Property.removeExpression( circle.property("ADBE Vector Ellipse Size") );
			}
		}

		var ikGroup = this.layer('ADBE Root Vectors Group').property("IK");
		if (ikGroup)
		{
			DuAEF.DuAE.Property.removeExpression(ikGroup.transform.scale);
			DuAEF.DuAE.Property.removeExpression(ikGroup.transform.position);
			DuAEF.DuAE.Property.removeExpression(ikGroup.transform.opacity);

			var ikContent = ikGroup.property("ADBE Vectors Group");

			var stroke = ikContent.property("ADBE Vector Graphic - Stroke");
			if (stroke)
			{
				DuAEF.DuAE.Property.removeExpression( stroke.property("ADBE Vector Stroke Color") );
			}
		}

		var ikLineGroup = this.layer('ADBE Root Vectors Group').property("IK Line");
		if (ikLineGroup)
		{
			var ikLineContent = ikLineGroup.property("ADBE Vectors Group");

			var stroke = ikLineContent.property("ADBE Vector Graphic - Stroke");
			if (stroke)
			{
				DuAEF.DuAE.Property.removeExpression( stroke.property("ADBE Vector Stroke Color") );
			}
		}

		var handleInGroup = this.layer('ADBE Root Vectors Group').property("Handle In");
		if (handleInGroup)
		{
			DuAEF.DuAE.Property.removeExpression(handleInGroup.transform.scale);

			var handleInContent = handleInGroup.property("ADBE Vectors Group");

			var fill = handleInContent.property("ADBE Vector Graphic - Fill");
			if (fill)
			{
				DuAEF.DuAE.Property.removeExpression( fill.property("ADBE Vector Fill Color") );
			}
		}

		var handleOutGroup = this.layer('ADBE Root Vectors Group').property("Handle Out");
		if (handleOutGroup)
		{
			DuAEF.DuAE.Property.removeExpression(handleOutGroup.transform.scale);

			var handleOutContent = handleOutGroup.property("ADBE Vectors Group");

			var fill = handleOutContent.property("ADBE Vector Graphic - Fill");
			if (fill)
			{
				DuAEF.DuAE.Property.removeExpression( fill.property("ADBE Vector Fill Color") );
			}
		}
	}

	if ( this.effect && this.effectInfo.isEffect )
	{
		this.effect = this.effectInfo.getProperty();
		try { if (this.effect.matchName == DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER) this.effect.remove() } catch (e) { if (DuAEF.debug) alert(e); };
	}
}

/**
	* Controllers
	* @namespace
	* @memberof DuAEF.Duik
*/
DuAEF.Duik.Controller = {};

/**
 * The shapes/type/icon of the controllers
 * @enum {int}
 * @readonly
 */
DuAEF.Duik.Controller.Types =
{
	ROTATION: 1,
	X_POSITION: 2,
	Y_POSITION: 3,
	POSITION: 4,
	TRANSFORM: 5,
	SLIDER: 6,
	DOUBLE_SLIDER: 7,
	ANGLE: 8,
	CAMERA: 9,
	HEAD: 10,
	HAND: 11,
	FOOT: 12,
	CLAWS: 13,
	HOOF: 14,
	EYE: 15,
	EYES: 16,
	FACE: 17,
	SQUARE: 18,
	HIPS: 19,
	BODY: 20,
	SHOULDERS: 21,
	TAIL: 22,
	PENIS: 23,
	VULVA: 24,
	WALK_CYCLE: 25,
	BLENDER: 26,
	NULL: 27,
	CONNECTOR: 28,
	EXPOSE_TRANSFORM: 29
}

/**
	* Size hint of controllers
	* @type {int}
	* @default DuAEF.Duik.Sizes.SMALL
*/
DuAEF.Duik.Controller.sizeHint = DuAEF.Duik.Sizes.SMALL;
/**
	* The default size for new controllers if controllerSizeHint is DuAEF.Duik.Sizes.CUSTOM
	* @type {int}
	* @default 100
*/
DuAEF.Duik.Controller.size = 100;
/**
	* The default color for new controllers
	* @type {float[]}
	* @default DuAEF.DuJS.Color.Colors.RAINBOX_RED
*/
DuAEF.Duik.Controller.color = DuAEF.DuJS.Color.Colors.RAINBOX_RED;
/**
	* The default type for new controllers
	* @type {DuAEF.Duik.Controller.Types}
	* @default DuAEF.Duik.Controller.Types.TRANSFORM
*/
DuAEF.Duik.Controller.defaultType = DuAEF.Duik.Controller.Types.TRANSFORM;
/**
	* The default quality for new controllers
	* @type {boolean}
	* @default false
*/
DuAEF.Duik.Controller.draft = false;

/**
	* Creates a new controller
	* @param {CompItem} [comp] - The composition
	* @param {DuAEF.Duik.Controller.Types} [type=DuAEF.Duik.Controller.defaultType] - The type of Controller.
	* @param {Layer|Layer[]|LayerCollection} [layer] - The layer(s) where to create the controller.<br />
	* If several layers are provided, will create the controller at the average center of their world positions.
	* @param {float}	[size] - The controller size, a percentage. By default, it will adjust automatically depending on comp size and DuAEF.Duik.Controller.sizeHint
	* @param {boolean} [parent=false] - True to automatically parent the layer(s) to the controller
	* @return {Controller} The controller
*/
DuAEF.Duik.Controller.create = function (comp, type, layer, size, color, parent)
{
	if (typeof comp === 'undefined')
	{
		if (typeof layer !== 'undefined')
		{
			comp = layer.containingComp;
		}
		else
		{
			comp = DuAEF.DuAE.Project.getActiveComp();
		}
	}
	if (!comp) return;

	if (typeof type === 'undefined') type = DuAEF.Duik.Controller.defaultType;
	if (typeof layer === 'undefined') layer = null;
	if (typeof parent === 'undefined') parent = false;

	var ctrl;
	if (type == DuAEF.Duik.Controller.Types.NULL)
	{
		ctrl = comp.layers.addNull();
		ctrl.transform.anchorPoint.setValue([50,50]);
	}
	else ctrl = comp.layers.addShape();
	DuAEF.Duik.setDuikMarker(ctrl,"Controller",DuAEF.Duik.LayerTypes.CONTROLLER);

	if (layer != null && type != DuAEF.Duik.Controller.Types.SLIDER && type != DuAEF.Duik.Controller.Types.DOUBLE_SLIDER && type != DuAEF.Duik.Controller.Types.ANGLE)
	{
		//if it's a null, switch to 3D if the layer is 3D
		var threeD = layer.threeDLayer && type == DuAEF.Duik.Controller.Types.NULL;
		ctrl.threeDLayer = threeD;

		//center the controller and name it after the layer(s)
		var pos = [0,0];
		var layerName = 'name';
		if (layer.length)
		{
			new Iterator(layer).do(function (l)
			{
				pos = pos + DuAEF.DuAE.Layer.getWorldPos(l);
			});
			pos = pos / layer.length;
		}
		else
		{
			pos = DuAEF.DuAE.Layer.getWorldPos(layer);
			layerName = layer.name;
		}
		
		ctrl.transform.position.setValue(pos);

		//name
		var newName = DuAEF.Duik.generateName('C',layerName);
		ctrl.name = DuAEF.DuAE.Comp.newUniqueLayerName(newName,comp);

		//parent
		if (parent) DuAEF.DuAE.Layer.parent(layer, ctrl);
	}

	DuAEF.Duik.setDuikMarkerParameter(ctrl,DuAEF.Duik.MarkerParameters.CONTROLLER_TYPE,type);

	DuAEF.Dugr.addLayerToGroup(ctrl,"Controllers");
	ctrl.guideLayer = true;
	ctrl.label = 9;
	DuAEF.DuAE.Layer.lockScale(ctrl);
	if (DuAEF.Duik.Controller.draft) ctrl.quality = LayerQuality.DRAFT;

	if (type == DuAEF.Duik.Controller.Types.NULL)
	{
		ctrl = new Controller(ctrl,type);
		return ctrl;
	}

	var effect = null;

	var pseudoEffect = null;

	if (type != DuAEF.Duik.Controller.Types.SQUARE && type != DuAEF.Duik.Controller.Types.SLIDER && type != DuAEF.Duik.Controller.Types.DOUBLE_SLIDER && type != DuAEF.Duik.Controller.Types.ANGLE)
	{
		//add effect
		effect = DuAEF.DuAE.Layer.addPseudoEffect(ctrl,DuAEF.Duik.PseudoEffects.CONTROLLER,DuAEF.Duik.PseudoEffectsMatchNames.CONTROLLER,'Controller');

		if (type != DuAEF.Duik.Controller.Types.WALK_CYCLE && type != DuAEF.Duik.Controller.Types.BLENDER && type != DuAEF.Duik.Controller.Types.CONNECTOR)
		{
			//create anchor
			var anchorGroup = ctrl("ADBE Root Vectors Group").addProperty("ADBE Vector Group");
			anchorGroup.name = 'Anchor';
			var anchorContent = anchorGroup.property("ADBE Vectors Group");
			var centerCircle = anchorContent.addProperty("ADBE Vector Shape - Ellipse");
			centerCircle("ADBE Vector Ellipse Size").setValue([2,2]);
			var biggerCircle = anchorContent.addProperty("ADBE Vector Shape - Ellipse");
			biggerCircle("ADBE Vector Ellipse Size").setValue([10,10]);
			var star = anchorContent.addProperty("ADBE Vector Shape - Star");
			star("ADBE Vector Star Points").setValue(4);
			star("ADBE Vector Star Inner Radius").setValue(0);
			star("ADBE Vector Star Outer Radius").setValue(10);
			var stroke = anchorContent.addProperty("ADBE Vector Graphic - Stroke");
			stroke("ADBE Vector Stroke Color").expression = 'effect("Controller")(9)';
			stroke("ADBE Vector Stroke Width").expression = 'var s = content("Anchor").transform.scale[0];\n' + 
				'if (s == 0) 0;\n' +
				'else 100/s;';
			stroke("ADBE Vector Stroke Line Cap").setValue(2);
			anchorGroup.transform.scale.expression = '[effect("Controller")(10),effect("Controller")(10)]';
		}

	}
	else if (type == DuAEF.Duik.Controller.Types.SLIDER)
	{
		pseudoEffect = DuAEF.Duik.PseudoEffects.CONTROLLER_SLIDER;
		//add effect
		effect = pseudoEffect.apply(ctrl);
	}
	else if (type == DuAEF.Duik.Controller.Types.DOUBLE_SLIDER)
	{
		pseudoEffect = DuAEF.Duik.PseudoEffects.CONTROLLER_DOUBLE_SLIDER;
		//add effect
		effect = pseudoEffect.apply(ctrl);
	}
	else if (type == DuAEF.Duik.Controller.Types.ANGLE)
	{
		pseudoEffect = DuAEF.Duik.PseudoEffects.CONTROLLER_ANGLE;
		//add effect
		effect = pseudoEffect.apply(ctrl);
	}

	var iconGroup = ctrl("ADBE Root Vectors Group").addProperty("ADBE Vector Group");
	iconGroup.name = 'Icon';
	var iconContent = iconGroup.property("ADBE Vectors Group");

	if (type != DuAEF.Duik.Controller.Types.SQUARE && type != DuAEF.Duik.Controller.Types.SLIDER && type != DuAEF.Duik.Controller.Types.DOUBLE_SLIDER && type != DuAEF.Duik.Controller.Types.ANGLE)
	{
		iconGroup.transform.scale.expression = '[effect("Controller")(4)*2,effect("Controller")(4)*2]';
		iconGroup.transform.position.expression = 'effect("Controller")(3)';
		iconGroup.transform.rotation.expression = 'effect("Controller")(5)';
		iconGroup.transform.opacity.expression = 'effect("Controller")(6)';
	}

	if (type == DuAEF.Duik.Controller.Types.ROTATION)
	{
		var shape = iconContent.addProperty("ADBE Vector Shape - Group");
		var rotationShape = new Shape();
		rotationShape.vertices = [[-8, 0.3499755859375], [0, 8.3499755859375], [8, 0.3499755859375], [0, -7.6500244140625], [4.80000305175781, -11.8499755859375]];
		rotationShape.inTangents = [[0, 0], [-4.41830444335938, 0], [0, 4.4183349609375], [4.41830444335938, 0], [0, 0]];
		rotationShape.outTangents = [[0, 4.4183349609375], [4.41830444335938, 0], [0, -4.4183349609375], [0, 0], [0, 0]];
		rotationShape.closed = false;
		shape('ADBE Vector Shape').setValue(rotationShape);
		var stroke = iconContent.addProperty("ADBE Vector Graphic - Stroke");
		stroke("ADBE Vector Stroke Color").expression = 'effect("Controller")(2)';
		stroke("ADBE Vector Stroke Width").setValue(1.5);
		stroke("ADBE Vector Stroke Line Cap").setValue(2);

		if (!layer) ctrl.name = DuAEF.DuAE.Comp.newUniqueLayerName('C | Rotation',comp);
	}
	else if (type == DuAEF.Duik.Controller.Types.X_POSITION)
	{
		var shape = iconContent.addProperty("ADBE Vector Shape - Group");
		var xPosShape = new Shape();
		xPosShape.vertices = [[12.5158386230469, -0.320068359375], [6.05984497070313, -6.776123046875], [5.74093627929688, -6.914794921875], [5.42213439941406, -6.776123046875], [4.729736328125, -6.083740234375], [4.59104919433594, -5.764892578125], [4.729736328125, -5.446044921875], [9.067138671875, -1.108642578125], [-9.05496215820313, -1.108642578125], [-4.74685668945313, -5.416748046875], [-4.60865783691406, -5.734619140625], [-4.74685668945313, -6.052490234375], [-5.43826293945313, -6.743896484375], [-5.75605773925781, -6.882080078125], [-6.07395935058594, -6.743896484375], [-12.516357421875, -0.301513671875], [-12.6545562744141, 0.016357421875], [-12.516357421875, 0.334228515625], [-6.07395935058594, 6.776611328125], [-5.75605773925781, 6.914794921875], [-5.43826293945313, 6.776611328125], [-4.74685668945313, 6.085205078125], [-4.60865783691406, 5.767333984375], [-4.74685668945313, 5.449462890625], [-9.30496215820313, 0.891357421875], [9.28193664550781, 0.891357421875], [4.729736328125, 5.443603515625], [4.59104919433594, 5.762451171875], [4.729736328125, 6.081298828125], [5.42213439941406, 6.773681640625], [5.74093627929688, 6.912353515625], [6.05984497070313, 6.773681640625], [12.5158386230469, 0.317626953125], [12.654541015625, -0.001220703125]];
