		var anchorGroup = ctrlBG("ADBE Root Vectors Group").addProperty("ADBE Vector Group");
		anchorGroup.name = 'Anchor';
		var anchorContent = anchorGroup.property("ADBE Vectors Group");
		var rectangle = anchorContent.addProperty("ADBE Vector Shape - Rect");
		rectangle("ADBE Vector Rect Size").expression = 'ctrl = thisComp.layer("' + ctrl.name + '");\nc = ctrl.effect("Slider")(' + pseudoEffect.index["HandleSizeSlider"] + ')/100*20;\nx = ctrl.effect("Slider")(' + pseudoEffect.index["SliderSizeSlider"] + ')+c;\n[x,x]';
		rectangle("ADBE Vector Rect Roundness").expression = 'thisComp.layer("' + ctrl.name + '").effect("Slider")(' + pseudoEffect.index["HandleSizeSlider"] + ')/100*10';
		var stroke = anchorContent.addProperty("ADBE Vector Graphic - Stroke");
		stroke("ADBE Vector Stroke Color").expression = 'thisComp.layer("' + ctrl.name + '").effect("Slider")(' + pseudoEffect.index["SliderColorColor"] + ')';
		stroke("ADBE Vector Stroke Width").expression = '100/content("Anchor").transform.scale[0]';
		stroke("ADBE Vector Stroke Line Cap").setValue(2);

		effect(pseudoEffect.index["X ValueSlider"]).expression = 'ctrl = thisComp.layer("' + ctrl.name + '");\n' +
						'x = ctrl.transform.position[0];\n' +
						'c = ctrl.effect("Slider")(' + pseudoEffect.index["HandleSizeSlider"] + ')/100*20;\n' +
						'h = ctrl.effect("Slider")(' + pseudoEffect.index["SliderSizeSlider"] + ')+c;\n' +
						'h = h-c;\n' +
						'h = h/2;\n' +
						'linear(x,-h,h,-100,100);';
		effect(pseudoEffect.index["Y ValueSlider"]).expression = 'ctrl = thisComp.layer("' + ctrl.name + '");\n' +
						'x = ctrl.transform.position[1];\n' +
						'c = ctrl.effect("Slider")(' + pseudoEffect.index["HandleSizeSlider"] + ')/100*20;\n' +
						'h = ctrl.effect("Slider")(' + pseudoEffect.index["SliderSizeSlider"] + ')+c;\n' +
						'h = h-c;\n' +
						'h = h/2;\n' +
						'linear(x,-h,h,-100,100);';
		effect( pseudoEffect.index["2D ValuePoint"] ).expression = '[effect("Slider")(' + pseudoEffect.index["X ValueSlider"] + '),effect("Slider")(' + pseudoEffect.index["Y ValueSlider"] + ')]';
	}
	else if (type == DuAEF.Duik.Controller.Types.ANGLE)
	{
		var shape1 = iconContent.addProperty("ADBE Vector Shape - Rect");
         shape1("ADBE Vector Rect Size").setValue([2,25]);
         shape1("ADBE Vector Rect Position").setValue([0,-12]);
		var fill = iconContent.addProperty("ADBE Vector Graphic - Fill");
		fill("ADBE Vector Fill Color").expression = 'effect("Angle")(2)';
        
		iconGroup.transform.opacity.expression = 'effect("Angle")(' + pseudoEffect.index["HandleOpacitySlider"] + ')';
		iconGroup.transform.scale.expression = '[effect("Angle")(' + pseudoEffect.index["HandleSizeSlider"] + ')*2,effect("Angle")(' + pseudoEffect.index["HandleSizeSlider"] + ')*2]';

		//create anchor
		var ctrlBG = comp.layers.addShape();
		ctrl.parent = ctrlBG;
		ctrlBG.moveAfter(ctrl);
		DuAEF.Duik.setDuikMarker(ctrlBG,"Controller Background",DuAEF.Duik.LayerTypes.CONTROLLER);
		ctrlBG.selected = false;
		ctrl.selected = true;
		ctrlBG.guideLayer = true;

		//name
		if (!layer)
		{
			ctrl.name = DuAEF.DuAE.Comp.newUniqueLayerName('C | Angle',comp);
			ctrlBG.name = DuAEF.DuAE.Comp.newUniqueLayerName('CBG | Angle',comp);
		}
		else
		{
			var newName = DuAEF.Duik.generateName('CBG',ctrl.name);
			ctrlBG.name = DuAEF.DuAE.Comp.newUniqueLayerName(newName,comp);
		}

		//create anchor
		var anchorGroup = ctrlBG("ADBE Root Vectors Group").addProperty("ADBE Vector Group");
		anchorGroup.name = 'Anchor';
		var anchorContent = anchorGroup.property("ADBE Vectors Group");
		var circle = anchorContent.addProperty("ADBE Vector Shape - Ellipse");
		circle("ADBE Vector Ellipse Size").expression = 'x = thisComp.layer("' + ctrl.name + '").effect("Angle")(' + pseudoEffect.index["SliderSizeSlider"] + ');\n[x,x]';
		var stroke = anchorContent.addProperty("ADBE Vector Graphic - Stroke");
		stroke("ADBE Vector Stroke Color").expression = 'thisComp.layer("' + ctrl.name + '").effect("Angle")(' + pseudoEffect.index["SliderColorColor"] + ')';
		stroke("ADBE Vector Stroke Width").expression = '100/content("Anchor").transform.scale[0]';
		stroke("ADBE Vector Stroke Line Cap").setValue(2);

		effect(pseudoEffect.index["AngleAngle"]).expression = 'thisComp.layer("' + ctrl.name + '").transform.rotation';
	}

	ctrl = new Controller(ctrl,type);
	ctrl.setSize(size);
	ctrl.setColor(color);
	return ctrl;
}

/**
 * Gets Controllers in the comp or the selection.
 * @param {CompItem}	[comp]	- The composition where to get the controllers
 * @param {boolean} [selectedOnly=false] - Selected layers only
 * @return {Controller[]}	The controllers.
 */
DuAEF.Duik.Controller.getControllers = function (comp,selectedOnly)
{
	if (typeof selectedOnly === 'undefined') selectedOnly = false;
	if (typeof comp === 'undefined') comp = DuAEF.DuAE.Project.getActiveComp();
	if (!comp) return [];

	var ctrls = [];
	//get layers
	var ctrlLayers = DuAEF.Duik.getLayers(DuAEF.Duik.LayerTypes.CONTROLLER,selectedOnly,comp);
	if (ctrlLayers.length == 0) return ctrls;
	//convert layers to Controller
	var it = new Iterator(ctrlLayers);
	it.do(function(layer){
		ctrls.push(new Controller(layer));
	})
	return ctrls;
}
