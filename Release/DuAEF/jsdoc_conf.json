{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
		"include": [ "DuAEF_full.jsxinc" ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
	"opts": {
        "template": "node_modules/ink-docstrap/template",
		"recurse": false,
        "destination": "../../docs/"
    },
	"templates": {
	"copyright"             : "Copyright (c) 2017 Nicolas Dufresne, Rainbox Productions",
	"includeDate"           : true,
	"theme"                 : "slate",
	"collapseSymbols"       : "false",
	"syntaxTheme"           : "dark",
	"search"                : "true"
	}
}
